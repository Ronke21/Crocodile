<link wikidata="Q3107574" start="79" end="93" title="Spongocoel" label="central cavity"/>
<link wikidata="Q18960" start="97" end="103" title="sponge" label="sponge"/>
<link wikidata="Q18960" start="166" end="171" title="Ostium (sponges)" label="ostia"/>
<link wikidata="Q2033176" start="211" end="218" title="osculum" label="osculum"/>
<link wikidata="Q18960" start="276" end="284" title="asconoid" label="asconoid"/>
<link wikidata="Q18960" start="286" end="294" title="syconoid" label="syconoid"/>
