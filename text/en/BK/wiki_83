<link wikidata="Q283992" start="10656" end="10658" title="ed (UNIX)" label="ed"/>
<link wikidata="Q219136" start="10887" end="10894" title="Hotmail" label="Hotmail"/>
<link wikidata="Q219136" start="10927" end="10938" title="Outlook.com" label="Outlook.com"/>
<link wikidata="Q1942216" start="10949" end="10959" title="Jack Smith (Hotmail)" label="Jack Smith"/>
<link wikidata="Q1650668" start="11049" end="11062" title="Sabeer Bhatia" label="Sabeer Bhatia"/>
