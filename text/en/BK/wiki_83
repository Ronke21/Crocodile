<link wikidata="None" start="7699" end="7720" title="Aleksander Sobolewski" label="Aleksander Sobolewski"/>
<link wikidata="Q112197" start="7751" end="7778" title="Righteous among the Nations" label="Righteous among the Nations"/>
<link wikidata="Q112197" start="7807" end="7834" title="Righteous among the Nations" label="Righteous among the Nations"/>
