<link wikidata="Q1817" start="336" end="359" title="Philip the Arab" label="Marcus Julius Philippus"/>
<link wikidata="Q842606" start="371" end="384" title="Roman Emperor" label="Roman Emperor"/>
<link wikidata="Q11767" start="630" end="641" title="Mesopotamia" label="Mesopotamia"/>
<link wikidata="Q207497" start="742" end="751" title="Macedonia (Roman province)" label="Macedonia"/>
