<link wikidata="Q344930" start="9770" end="9779" title="Evan Bayh" label="Evan Bayh"/>
<link wikidata="Q532220" start="9871" end="9884" title="Steve Beshear" label="Steve Beshear"/>
<link wikidata="Q1603" start="9960" end="9968" title="Kentucky" label="Kentucky"/>
<link wikidata="Q21572825" start="9989" end="9993" title="Andy Beshear" label="Andy"/>
