<link wikidata="Q2575646" start="3053" end="3062" title="Qahtanite" label="Qahtanite"/>
<link wikidata="Q27174" start="3131" end="3137" title="Najran" label="Najran"/>
<link wikidata="Q965927" start="3219" end="3231" title="George Moore (novelist)" label="George Moore"/>
