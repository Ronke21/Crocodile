<link wikidata="Q1505" start="49" end="55" title="Sikkim" label="Sikkim"/>
<link wikidata="Q972684" start="154" end="167" title="Lepcha people" label="Lepcha people"/>
<link wikidata="Q972684" start="245" end="252" title="Lepcha people" label="Lepchas"/>
