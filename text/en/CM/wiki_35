<link wikidata="Q5295" start="28" end="48" title="random access memory" label="random access memory"/>
<link wikidata="Q515636" start="70" end="74" title="FIFO (computing and electronics)" label="FIFO"/>
<link wikidata="Q2492305" start="75" end="86" title="Consistency model" label="consistency"/>
<link wikidata="Q205663" start="99" end="108" title="Process (computing)" label="processes"/>
<link wikidata="Q66221" start="113" end="119" title="Computer storage" label="memory"/>
<link wikidata="Q1319773" start="316" end="326" title="consistent" label="consistent"/>
<link wikidata="Q4379351" start="783" end="801" title="Causal consistency" label="Causal consistency"/>
</links>
