<link wikidata="Q6962086" start="15346" end="15352" title="Namsos (town)" label="Namsos"/>
<link wikidata="Q271083" start="15357" end="15366" title="Åndalsnes" label="Åndalsnes"/>
<link wikidata="Q3081812" start="15448" end="15462" title="Operation Alphabet" label="the evacuation"/>
