<link wikidata="Q2073960" start="10572" end="10586" title="Craig Robinson (basketball coach)" label="Craig Robinson"/>
<link wikidata="Q4382677" start="11623" end="11638" title="Maya Soetoro-Ng" label="Maya Soetoro-Ng"/>
<link wikidata="Q76" start="11690" end="11702" title="Barack Obama" label="Barack Obama"/>
<link wikidata="Q616886" start="11916" end="11925" title="Ken Burns" label="Ken Burns"/>
<link wikidata="Q230303" start="11967" end="11983" title="Caroline Kennedy" label="Caroline Kennedy"/>
