<link wikidata="None" start="68" end="77" title=":wikt:statement" label="statement"/>
<link wikidata="Q7949" start="135" end="139" title="truth" label="true"/>
<link wikidata="Q454263" start="204" end="213" title="affidavit" label="affidavit"/>
<link wikidata="Q381045" start="236" end="241" title="oath" label="sworn"/>
<link wikidata="Q15479268" start="254" end="267" title="notary public" label="notary public"/>
<link wikidata="Q310229" start="501" end="508" title="perjury" label="perjury"/>
