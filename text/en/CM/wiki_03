<link wikidata="Q1319139" start="6921" end="6935" title="Kung-Fu Master (video game)" label="Kung-Fu Master"/>
<link wikidata="Q617752" start="6957" end="6961" title="Irem" label="Irem"/>
<link wikidata="Q3142082" start="7068" end="7081" title="Double Dragon" label="Double Dragon"/>
<link wikidata="Q2752616" start="7090" end="7098" title="Pac-Land" label="Pac-Land"/>
<link wikidata="Q309996" start="7120" end="7125" title="Namco" label="Namco"/>
<link wikidata="Q593781" start="7179" end="7185" title="Flicky" label="Flicky"/>
