<link wikidata="Q93195" start="3947" end="3953" title="Ulster" label="Ulster"/>
<link wikidata="Q201966" start="4094" end="4106" title="Ulster Scots dialects" label="Ulster Scots"/>
<link wikidata="None" start="4128" end="4157" title="Ulster Scots Language Society" label="Ulster Scots Language Society"/>
