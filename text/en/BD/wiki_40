<link wikidata="Q3458244" start="22132" end="22156" title="Additional Articles of the Constitution of the Republic of China" label="constitutional amendment"/>
<link wikidata="Q715869" start="22178" end="22202" title="Legislative Yuan" label="Taiwan&amp;#x27;s parliament"/>
<link wikidata="Q7307171" start="22207" end="22218" title="Referendums in Taiwan" label="referendums"/>
<link wikidata="Q245107" start="22257" end="22270" title="free area of the Republic of China" label="ROC free area"/>
