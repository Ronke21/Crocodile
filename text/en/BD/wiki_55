<link wikidata="Q179224" start="10778" end="10783" title="Dover" label="Dover"/>
<link wikidata="Q129247" start="11057" end="11067" title="Edward III of England" label="Edward III"/>
<link wikidata="Q187312" start="11770" end="11786" title="Robert the Bruce" label="Robert the Bruce"/>
