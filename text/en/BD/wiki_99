<link wikidata="Q4962539" start="126" end="150" title="Brevet d&amp;#x27;état-major" label="Brevet d&amp;#x27;état-major"/>
<link wikidata="Q4698579" start="250" end="264" title="Aircrew brevet" label="Aircrew brevet"/>
