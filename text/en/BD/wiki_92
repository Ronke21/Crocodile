<link wikidata="Q96" start="6142" end="6149" title="Mexico" label="Mexican"/>
<link wikidata="Q975" start="6581" end="6599" title="San Antonio" label="San Antonio, Texas"/>
<link wikidata="Q1588" start="6629" end="6638" title="Louisiana" label="Louisiana"/>
<link wikidata="Q2278195" start="6642" end="6654" title="Natchitoches, Louisiana" label="Natchitoches"/>
