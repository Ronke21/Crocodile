<link wikidata="Q434352" start="470" end="482" title="Chad Hedrick" label="Chad Hedrick"/>
<link wikidata="Q779272" start="970" end="979" title="ice skating" label="ice skate"/>
<link wikidata="Q434352" start="4864" end="4876" title="Chad Hedrick" label="Chad Hedrick"/>
