<link wikidata="Q63020424" start="5174" end="5196" title="De Persgroep Nederland" label="De Persgroep Nederland"/>
<link wikidata="Q759811" start="6029" end="6040" title="Newspaper circulation" label="circulation"/>
<link wikidata="Q1898796" start="6156" end="6168" title="Mario García (designer)" label="Mario Garcia"/>
<link wikidata="Q1535869" start="6235" end="6250" title="Jip and Janneke" label="Jip and Janneke"/>
