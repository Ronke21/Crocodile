<link wikidata="Q14118" start="197" end="202" title="Savoy" label="Savoy"/>
<link wikidata="Q495" start="234" end="239" title="Turin" label="Turin"/>
<link wikidata="None" start="256" end="271" title="Lascaris Palace" label="Lascaris Palace"/>
</links>
