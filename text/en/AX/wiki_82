<link wikidata="Q1353297" start="15237" end="15264" title="RFA Sir Galahad (1987)" label="RFA &amp;quot;Sir Galahad&amp;quot;"/>
<link wikidata="Q842407" start="15304" end="15312" title="Umm Qasr" label="Umm Qasr"/>
<link wikidata="Q1046947" start="15362" end="15375" title="Royal Marines" label="Royal Marines"/>
<link wikidata="Q4637752" start="15392" end="15403" title="42 Commando" label="42 Commando"/>
<link wikidata="Q1142173" start="15459" end="15474" title="Fedayeen Saddam" label="Fedayeen Saddam"/>
