<link wikidata="Q1200072" start="10286" end="10289" title="NIV" label="NIV"/>
<link wikidata="Q201410" start="10297" end="10313" title="deuterocanonical" label="deuterocanonical"/>
<link wikidata="Q155980" start="10314" end="10328" title="Book of Sirach" label="Book of Sirach"/>
<link wikidata="Q8018" start="10623" end="10632" title="Augustine" label="Augustine"/>
<link wikidata="Q43689" start="10638" end="10645" title="Ambrose" label="Ambrose"/>
<link wikidata="Q161775" start="10720" end="10727" title="Saint Stephen" label="Stephen"/>
<link wikidata="Q3742239" start="10733" end="10746" title="Felix of Nola" label="Felix of Nola"/>
