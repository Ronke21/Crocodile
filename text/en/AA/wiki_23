<link wikidata="Q12899451" start="5091" end="5112" title="Symposium (Feuerbach)" label="Symposium (Feuerbach)"/>
<link wikidata="Q84" start="5263" end="5269" title="London" label="London"/>
<link wikidata="Q43353" start="5365" end="5377" title="Aristophanes" label="Aristophanes"/>
