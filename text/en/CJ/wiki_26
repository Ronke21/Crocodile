<link wikidata="Q118093" start="283" end="295" title="Transmission (telecommunications)" label="transmission"/>
<link wikidata="Q377672" start="596" end="605" title="rise time" label="rise time"/>
<link wikidata="Q128831" start="967" end="1000" title="Federal Communications Commission" label="Federal Communications Commission"/>
<link wikidata="None" start="1108" end="1131" title="Subscriber Mobile Radio" label="Subscriber Mobile Radio"/>
