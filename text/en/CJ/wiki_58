<link wikidata="Q424" start="4667" end="4675" title="Cambodia" label="Cambodia"/>
<link wikidata="Q485921" start="4717" end="4726" title="COMUSMACV" label="COMUSMACV"/>
<link wikidata="Q280290" start="4735" end="4751" title="Creighton Abrams" label="Creighton Abrams"/>
<link wikidata="Q7139834" start="4913" end="4941" title="Parrot&amp;#x27;s Beak, Cambodia" label="Parrot&amp;#x27;s Beak, Cambodia"/>
<link wikidata="Q96743099" start="5041" end="5054" title="Hal D. McCown" label="Hal D. McCown"/>
