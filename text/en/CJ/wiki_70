<link wikidata="Q4748155" start="475" end="493" title="amphenol connector" label="amphenol connector"/>
<link wikidata="Q854506" start="627" end="637" title="line level" label="line level"/>
<link wikidata="Q5629611" start="987" end="1001" title="HH Electronics" label="HH Electronics"/>
