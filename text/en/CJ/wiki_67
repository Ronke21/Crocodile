<link wikidata="Q795579" start="3626" end="3631" title="BBC 7" label="BBC 7"/>
<link wikidata="Q1782553" start="4036" end="4048" title="Heron Carvic" label="Heron Carvic"/>
<link wikidata="Q4708036" start="4236" end="4249" title="Alan Wheatley" label="Alan Wheatley"/>
<link wikidata="Q23308" start="4823" end="4838" title="British Library" label="British Library"/>
