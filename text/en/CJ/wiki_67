<link wikidata="Q1285613" start="16402" end="16417" title="Spencer Abraham" label="Spencer Abraham"/>
<link wikidata="Q731392" start="16450" end="16468" title="Milton Robert Carr" label="Milton Robert Carr"/>
<link wikidata="Q1124" start="16650" end="16672" title="Bill Clinton" label="President Bill Clinton"/>
