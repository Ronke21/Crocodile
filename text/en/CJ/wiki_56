<link wikidata="Q12827256" start="106" end="115" title="mythology" label="mythology"/>
<link wikidata="Q782" start="119" end="125" title="Hawaii" label="Hawaii"/>
<link wikidata="Q7245" start="504" end="518" title="Samuel Clemens" label="Samuel Clemens"/>
<link wikidata="Q7245" start="536" end="546" title="Mark Twain" label="Mark Twain"/>
<link wikidata="Q1132427" start="622" end="637" title="Summer of Night" label="Summer of Night"/>
</links>
