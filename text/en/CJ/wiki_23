<link wikidata="Q11220" start="100" end="118" title="United States Navy" label="United States Navy"/>
<link wikidata="Q309252" start="136" end="152" title="physical fitness" label="physical fitness"/>
<link wikidata="Q178033" start="1365" end="1369" title="SEAL" label="SEAL"/>
<link wikidata="Q1560626" start="1371" end="1375" title="Special Warfare Combatant-craft Crewmen" label="SWCC"/>
<link wikidata="Q7890816" start="1377" end="1387" title="United States Navy Diver" label="Navy Diver"/>
