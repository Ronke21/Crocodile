<link wikidata="Q1238280" start="4" end="14" title="Sea Ferret" label="Sea Ferret"/>
<link wikidata="Q11220" start="93" end="111" title="United States Navy" label="United States Navy"/>
<link wikidata="Q466704" start="504" end="526" title="surface-to-air missile" label="surface-to-air missile"/>
<link wikidata="Q329953" start="575" end="591" title="Northrop Grumman" label="Northrop Grumman"/>
<link wikidata="Q1272420" start="658" end="689" title="USS Asheville (SSN-758)" label="&amp;quot;Asheville&amp;quot; (SSN-758)"/>
