<link wikidata="Q7391998" start="99" end="104" title="SOBER (cipher)" label="SOBER"/>
<link wikidata="Q4681865" start="115" end="121" title="cipher" label="cipher"/>
<link wikidata="Q1052621" start="163" end="166" title="Message authentication code" label="MAC"/>
<link wikidata="Q471771" start="451" end="454" title="key (cryptography)" label="key"/>
<link wikidata="Q3901296" start="497" end="502" title="Helix (cipher)" label="Helix"/>
