<link wikidata="Q1162538" start="63" end="72" title="plaintext" label="plaintext"/>
<link wikidata="Q141090" start="80" end="90" title="encryption" label="encryption"/>
<link wikidata="Q700583" start="108" end="182" title="Known-plaintext attack" label="stereotyped headers, salutations, introductions, endings, signatures, etc."/>
<link wikidata="Q897511" start="209" end="221" title="cryptanalyst" label="cryptanalyst"/>
<link wikidata="Q840661" start="373" end="404" title="ciphertext indistinguishability" label="ciphertext indistinguishability"/>
