{ &quot;type&quot;: &quot;Feature&quot;,
 &quot;geometry&quot;: { &quot;type&quot;: &quot;Point&quot;, &quot;coordinates&quot;: [35.5161, 31.8326] },
 &quot;properties&quot;: {
 &quot;title&quot;: &quot;Gilgal Yericho (?)&quot;,
