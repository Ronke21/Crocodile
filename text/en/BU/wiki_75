<link wikidata="Q3345634" start="3899" end="3918" title="Ramayan (1987 TV series)" label="&amp;quot;Ramayan&amp;quot;"/>
<link wikidata="Q3632712" start="4031" end="4045" title="Rajendra Kumar" label="Rajendra Kumar"/>
<link wikidata="Q107130" start="4070" end="4083" title="Rajesh Khanna" label="Rajesh Khanna"/>
