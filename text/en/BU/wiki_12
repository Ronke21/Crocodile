<link wikidata="Q128234" start="41" end="57" title="grammatical case" label="grammatical case"/>
<link wikidata="Q9067" start="150" end="159" title="Hungarian language" label="Hungarian"/>
<link wikidata="Q492457" start="409" end="426" title="distributive case" label="distributive case"/>
