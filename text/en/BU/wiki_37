<link wikidata="Q80005" start="32" end="36" title="fern" label="fern"/>
<link wikidata="Q1358407" start="54" end="58" title="sorus" label="sori"/>
<link wikidata="Q1141315" start="200" end="211" title="Pteridaceae" label="Pteridaceae"/>
