<link wikidata="Q128196" start="31" end="45" title="Mie Prefecture" label="Mie Prefecture"/>
<link wikidata="Q17" start="47" end="52" title="Japan" label="Japan"/>
<link wikidata="Q2625603" start="97" end="107" title="population" label="population"/>
<link wikidata="Q22856" start="124" end="142" title="population density" label="population density"/>
