<link wikidata="Q19707" start="4659" end="4664" title="burro" label="burro"/>
<link wikidata="Q102083" start="4772" end="4781" title="Knight" label="caballero"/>
<link wikidata="Q3649129" start="4949" end="4955" title="cabana (structure)" label="cabana"/>
