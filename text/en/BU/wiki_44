<link wikidata="Q1198236" start="8922" end="8939" title="Compulsion (1959 film)" label="1959 film version"/>
<link wikidata="Q24829" start="8967" end="8979" title="Orson Welles" label="Orson Welles"/>
<link wikidata="Q896953" start="8984" end="9000" title="Bradford Dillman" label="Bradford Dillman"/>
<link wikidata="Q42369" start="9017" end="9023" title="Cannes Film Festival" label="Cannes"/>
