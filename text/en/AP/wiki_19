<link wikidata="Q1806741" start="121" end="125" title="Māui (Māori mythology)" label="Māui"/>
<link wikidata="Q118863" start="143" end="155" title="North Island" label="North Island"/>
<link wikidata="Q664" start="159" end="170" title="New Zealand" label="New Zealand"/>
<link wikidata="Q15983991" start="610" end="618" title="John White (ethnographer)" label="J. White"/>
