<link wikidata="Q131443" start="11476" end="11483" title="Lakshmi" label="Lakshmi"/>
<link wikidata="Q2668661" start="11517" end="11532" title="Uchchaihshravas" label="Uchchaihshravas"/>
<link wikidata="Q440861" start="11553" end="11561" title="Airavata" label="Airavata"/>
