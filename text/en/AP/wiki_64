<link wikidata="Q491911" start="43" end="54" title="Hart County, Kentucky" label="Hart County"/>
<link wikidata="Q1603" start="56" end="64" title="Kentucky" label="Kentucky"/>
<link wikidata="Q778747" start="138" end="148" title="Horse Cave, Kentucky" label="Horse Cave"/>
<link wikidata="Q409476" start="152" end="166" title="U.S. Route 31E" label="U.S. Route 31E"/>
