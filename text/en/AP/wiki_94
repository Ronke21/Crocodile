<link wikidata="Q312766" start="3617" end="3623" title="akutaq" label="akutaq"/>
<link wikidata="Q276303" start="3689" end="3698" title="mousefood" label="mousefood"/>
<link wikidata="Q249775" start="3700" end="3712" title="Labrador tea" label="Labrador tea"/>
<link wikidata="Q332777" start="3733" end="3741" title="Rumex arcticus" label="sourdock"/>
<link wikidata="Q1010563" start="4013" end="4019" title="Bureau of Indian Affairs" label="B.I.A."/>
<link wikidata="Q55621382" start="4104" end="4135" title="Lower Kuskokwim School District" label="Lower Kuskokwim School District"/>
<link wikidata="Q17240" start="4666" end="4676" title="snowmobile" label="snowmobile"/>
<link wikidata="Q1708562" start="4805" end="4829" title="Chefornak Airport" label="Chefornak&amp;#x27;s airport"/>
<link wikidata="Q19754880" start="5106" end="5115" title="Paul John (Yupik elder)" label="Paul John"/>
<link wikidata="Q37014" start="5129" end="5140" title="Yupik peoples" label="Yup&amp;#x27;ik"/>
</links>
