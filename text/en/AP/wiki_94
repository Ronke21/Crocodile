<link wikidata="Q3127419" start="3303" end="3310" title="herring" label="herring"/>
<link wikidata="Q1072897" start="3343" end="3348" title="jerky" label="jerky"/>
<link wikidata="Q80838" start="3354" end="3370" title="Alaska blackfish" label="Alaska blackfish"/>
<link wikidata="Q160092" start="3487" end="3515" title="Rubus chamaemorus" label="salmonberries (cloudberries)"/>
