<link wikidata="Q3848625" start="36" end="43" title="Pouākai" label="Pouākai"/>
<link wikidata="Q243451" start="83" end="101" title="Harpagornis" label="Haast&amp;#x27;s Eagle"/>
<link wikidata="Q31675204" start="136" end="149" title="Pouakai Range" label="Pouakai Range"/>
