<link wikidata="Q1633051" start="5190" end="5200" title="pink slime" label="pink slime"/>
<link wikidata="Q135888" start="5273" end="5290" title="Chicken McNuggets" label="Chicken McNuggets"/>
<link wikidata="Q27881073" start="5617" end="5634" title="fake news website" label="fake news website"/>
<link wikidata="Q5032594" start="5766" end="5786" title="cannabis consumption" label="cannabis consumption"/>
<link wikidata="Q28136147" start="5835" end="5845" title="Now 8 News" label="Now 8 News"/>
