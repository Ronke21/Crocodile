<link wikidata="Q2857020" start="2057" end="2093" title="common-tone diminished seventh chord" label="common-tone diminished seventh chord"/>
<link wikidata="Q837294" start="2264" end="2286" title="enharmonic equivalence" label="enharmonic equivalence"/>
<link wikidata="Q7333124" start="2483" end="2500" title="Riemannian theory" label="Riemannian theory"/>
