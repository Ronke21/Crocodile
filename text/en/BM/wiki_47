<link wikidata="Q421" start="104" end="108" title="UFOs" label="UFOs"/>
<link wikidata="Q3267576" start="117" end="131" title="Shaver Mystery" label="Shaver Mystery"/>
<link wikidata="Q3887092" start="382" end="394" title="Other Worlds (magazine)" label="Other Worlds"/>
<link wikidata="Q2610159" start="1247" end="1265" title="Amherst, Wisconsin" label="Amherst, Wisconsin"/>
<link wikidata="Q462799" start="1333" end="1351" title="Evanston, Illinois" label="Evanston, Illinois"/>
</links>
