<link wikidata="Q694007" start="202" end="228" title="Learning Management System" label="Learning Management System"/>
<link wikidata="Q2115" start="414" end="417" title="XML" label="XML"/>
<link wikidata="Q42253" start="629" end="632" title="Uniform Resource Locator" label="URL"/>
