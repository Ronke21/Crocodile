<link wikidata="Q3847357" start="68594" end="68603" title="Variation (ballet)" label="variation"/>
<link wikidata="Q194363" start="68669" end="68684" title="Vaslav Nijinsky" label="Vaslav Nijinsky"/>
<link wikidata="None" start="68993" end="68999" title="#Dégagé" label="dégagé"/>
