<link wikidata="Q6730914" start="38" end="44" title="Magic cookie" label="cookie"/>
<link wikidata="Q40056" start="120" end="128" title="Computer program" label="programs"/>
<link wikidata="Q3291944" start="180" end="185" title="Event (computing)" label="event"/>
<link wikidata="Q7095588" start="384" end="390" title="opaque data type" label="opaque"/>
<link wikidata="Q506053" start="650" end="656" title="Ticket (IT security)" label="ticket"/>
<link wikidata="Q47512572" start="724" end="732" title="man page" label="man page"/>
