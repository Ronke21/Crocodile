<link wikidata="Q900842" start="11355" end="11359" title="1995 World Rally Championship season" label="1995"/>
<link wikidata="Q201291" start="11530" end="11541" title="Colin McRae" label="Colin McRae"/>
<link wikidata="Q966789" start="11573" end="11588" title="Rally Catalunya" label="Rally Catalunya"/>
<link wikidata="Q1496877" start="11764" end="11772" title="Pacenotes" label="pacenote"/>
