<link wikidata="" start="203" end="235" title="18 Wheeler: American Pro Trucker" label="18 Wheeler: American Pro Trucker"/>
<link wikidata="Q159270" start="313" end="326" title="Missundaztood" label="Missundaztood"/>
<link wikidata="Q179" start="335" end="345" title="Boeing 747" label="Boeing 747"/>
<link wikidata="Q7360880" start="389" end="415" title="Roll On (Eighteen Wheeler)" label="Roll On (Eighteen Wheeler)"/>
</links>
