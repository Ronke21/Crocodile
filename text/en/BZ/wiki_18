<link wikidata="Q1930" start="94" end="100" title="Ottawa" label="Ottawa"/>
<link wikidata="Q1904" start="102" end="109" title="Ontario" label="Ontario"/>
<link wikidata="Q5029304" start="136" end="154" title="Canada 2011 Census" label="Canada 2011 Census"/>
