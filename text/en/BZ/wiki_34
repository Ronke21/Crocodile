<link wikidata="Q1047000" start="355" end="360" title="Validity (logic)" label="valid"/>
<link wikidata="Q6667497" start="361" end="373" title="logical form" label="logical form"/>
<link wikidata="None" start="776" end="800" title="Hyponymy and hypernymy#Transitivity of hyponymy" label="Transitivity of hyponymy"/>
<link wikidata="None" start="803" end="895" title="Syllogism#Other types" label="Type of syllogism (disjunctive, hypothetical, legal, poly-, prosleptic, quasi-, statistical)"/>
</links>
