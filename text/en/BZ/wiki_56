<link wikidata="Q1574099" start="82" end="102" title="radiation protection" label="radiation protection"/>
<link wikidata="Q6508434" start="108" end="122" title="lead shielding" label="lead shielding"/>
<link wikidata="Q1362384" start="720" end="745" title="certified first responder" label="certified first responder"/>
<link wikidata="Q1641373" start="746" end="758" title="hazmat suits" label="hazmat suits"/>
