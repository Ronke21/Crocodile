<link wikidata="Q336379" start="34" end="73" title="Robert Vansittart, 1st Baron Vansittart" label="Robert Vansittart, 1st Baron Vansittart"/>
<link wikidata="Q7350580" start="105" end="130" title="Robert Vansittart (judge)" label="Robert Vansittart (judge)"/>
<link wikidata="Q5729491" start="218" end="234" title="Henry Vansittart" label="Henry Vansittart"/>
