<link wikidata="Q459752" start="17323" end="17333" title="Hugo Black" label="Hugo Black"/>
<link wikidata="Q28112878" start="17475" end="17492" title="Lucas A. Powe Jr." label="Lucas A. Powe Jr."/>
<link wikidata="Q1779167" start="18490" end="18497" title="Legal realism" label="realist"/>
