<link wikidata="Q1060566" start="14084" end="14130" title="Schomburg Center for Research in Black Culture" label="Schomburg Center for Research in Black Culture"/>
<link wikidata="Q219555" start="14132" end="14155" title="New York Public Library" label="New York Public Library"/>
<link wikidata="Q1601838" start="14207" end="14228" title="Spencer Museum of Art" label="Spencer Museum of Art"/>
