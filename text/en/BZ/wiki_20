<link wikidata="Q5891" start="29" end="42" title="Philosophy" label="philosophical"/>
<link wikidata="Q100235738" start="74" end="79" title="event (philosophy)" label="event"/>
<link wikidata="Q179289" start="94" end="100" title="causality" label="causes"/>
<link wikidata="Q202834" start="189" end="205" title="supervenience" label="non-supervenient"/>
<link wikidata="Q112812" start="1186" end="1202" title="epiphenomenalism" label="epiphenomenalism"/>
</links>
