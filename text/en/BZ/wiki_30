<link wikidata="Q6764498" start="10562" end="10586" title="Mario&amp;#x27;s Green House" label="Mario&amp;#x27;s Green House"/>
<link wikidata="Q3327694" start="10714" end="10731" title="Multiple Sarcasms" label="Multiple Sarcasms"/>
