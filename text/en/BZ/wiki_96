<link wikidata="Q54174" start="147" end="168" title="Woburn, Massachusetts" label="Woburn, Massachusetts"/>
<link wikidata="Q49123" start="280" end="295" title="Harvard College" label="Harvard College"/>
<link wikidata="Q11806" start="379" end="389" title="John Adams" label="John Adams"/>
<link wikidata="Q591912" start="732" end="755" title="Sherborn, Massachusetts" label="Sherborn, Massachusetts"/>
<link wikidata="Q19588706" start="774" end="805" title="president of Harvard University" label="president of Harvard University"/>
