<link wikidata="Q1055911" start="36" end="48" title="chess tactic" label="chess tactic"/>
<link wikidata="Q961090" start="213" end="222" title="Michał Krasenkow" label="Krasenkow"/>
<link wikidata="Q131674" start="223" end="229" title="Anatoly Karpov" label="Karpov"/>
