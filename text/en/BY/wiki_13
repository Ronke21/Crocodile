<link wikidata="Q4774127" start="30" end="33" title="Anti-Capitalist Convergence" label="ACC"/>
<link wikidata="Q49" start="72" end="85" title="North America" label="North America"/>
<link wikidata="Q264892" start="171" end="185" title="social justice" label="social justice"/>
<link wikidata="Q6199" start="187" end="196" title="anarchist" label="anarchist"/>
<link wikidata="Q2855609" start="202" end="218" title="environmentalist" label="environmentalist"/>
<link wikidata="Q389647" start="219" end="235" title="anti-capitalism" label="anti-capitalists"/>
