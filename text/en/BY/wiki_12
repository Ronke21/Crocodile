<link wikidata="Q779" start="1701" end="1712" title="Connecticut" label="Connecticut"/>
<link wikidata="Q79435739" start="1765" end="1779" title="James Magruder" label="James Magruder"/>
<link wikidata="Q49112" start="2101" end="2116" title="Yale University" label="Yale University"/>
