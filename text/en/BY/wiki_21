<link wikidata="Q2575910" start="85" end="95" title="Uji (clan)" label="Uji (clan)"/>
<link wikidata="Q69817789" start="151" end="167" title="Uji (Being-Time)" label="Uji (Being-Time)"/>
<link wikidata="Q29718" start="231" end="249" title="Jaume I University" label="Jaume I University"/>
<link wikidata="Q109228620" start="303" end="310" title="Uzi fly" label="Uzi fly"/>
<link wikidata="Q7080021" start="408" end="411" title="Ogi (food)" label="ogi"/>
