<link wikidata="Q192588" start="178" end="198" title="relational databases" label="relational databases"/>
<link wikidata="Q278425" start="242" end="247" title="Table (SQL)" label="table"/>
<link wikidata="Q1329910" start="251" end="255" title="View (SQL)" label="view"/>
<link wikidata="Q180160" start="319" end="327" title="metadata" label="metadata"/>
