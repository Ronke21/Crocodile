<link wikidata="Q837985" start="59" end="77" title="Middle High German" label="Middle High German"/>
<link wikidata="Q980" start="272" end="279" title="Bavaria" label="Bavaria"/>
<link wikidata="Q4457" start="374" end="391" title="Franz Beckenbauer" label="Franz Beckenbauer"/>
<link wikidata="Q458888" start="424" end="443" title="Stephan Beckenbauer" label="Stephan Beckenbauer"/>
<link wikidata="Q2015154" start="516" end="537" title="Franz Beckenbauer Cup" label="Franz Beckenbauer Cup"/>
<link wikidata="Q4878641" start="586" end="605" title="Beckenbauer Seur-In" label="Beckenbauer Seur-In"/>
</links>
