<link wikidata="Q1048898" start="16604" end="16625" title="University of Vermont" label="University of Vermont"/>
<link wikidata="Q49165" start="16775" end="16790" title="Amherst College" label="Amherst College"/>
<link wikidata="Q453218" start="16876" end="16894" title="Dwight Lyman Moody" label="Dwight Lyman Moody"/>
<link wikidata="Q7059380" start="16902" end="16930" title="Northfield Mount Hermon School" label="Mount Hermon School for Boys"/>
<link wikidata="Q7996607" start="16991" end="17006" title="Whitman College" label="Whitman College"/>
