<link wikidata="Q192089" start="56" end="68" title="magic square" label="magic square"/>
<link wikidata="Q2940397" start="1691" end="1706" title="panmagic square" label="panmagic square"/>
<link wikidata="Q467786" start="1840" end="1860" title="Kathleen Ollerenshaw" label="Kathleen Ollerenshaw"/>
<link wikidata="None" start="1865" end="1878" title="David S. Brée" label="David S. Brée"/>
