<link wikidata="Q464674" start="2474" end="2489" title="François Louis, Prince of Conti" label="Francois-Louis."/>
<link wikidata="Q622641" start="2694" end="2729" title="Élisabeth Marguerite d&amp;#x27;Orléans" label="Élisabeth Marguerite d&amp;#x27;Orléans"/>
<link wikidata="Q644635" start="2743" end="2764" title="Gaston, Duke of Orléans" label="Gaston d&amp;#x27;Orléans"/>
<link wikidata="Q65146" start="2864" end="2889" title="Anne Henriette of Bavaria" label="Anne Henriette of Bavaria"/>
<link wikidata="Q1075988" start="2911" end="2927" title="Palais du Louvre" label="Palais du Louvre"/>
<link wikidata="Q62998" start="2985" end="3018" title="Edward, Count Palatine of Simmern" label="Edward, Count Palatine of Simmern"/>
<link wikidata="Q2556248" start="3045" end="3057" title="Anna Gonzaga" label="Anna Gonzaga"/>
<link wikidata="Q122981" start="3529" end="3536" title="anagram" label="anagram"/>
<link wikidata="Q1287" start="4371" end="4380" title="Jansenism" label="Jansenist"/>
<link wikidata="Q447520" start="4577" end="4597" title="Louis III, Prince of Condé" label="Louis III de Bourbon"/>
<link wikidata="Q455392" start="4695" end="4706" title="Louis Henri, Duke of Bourbon" label="Louis-Henri"/>
