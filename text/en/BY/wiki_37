<link wikidata="Q17058590" start="12418" end="12432" title="Squire Sanders" label="Squire Sanders"/>
<link wikidata="Q43196" start="12498" end="12508" title="Cincinnati" label="Cincinnati"/>
<link wikidata="Q2101438" start="12668" end="12674" title="political action committee" label="P.A.C."/>
