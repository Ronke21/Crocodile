<link wikidata="Q5208942" start="18430" end="18442" title="dahong palay" label="dahong palay"/>
<link wikidata="Q6350738" start="18541" end="18548" title="kalasag" label="kalasag"/>
<link wikidata="Q2091449" start="18561" end="18577" title="Battle of Mactan" label="Battle of Mactan"/>
