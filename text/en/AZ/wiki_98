<link wikidata="Q242388" start="2794" end="2803" title="Flamsteed" label="Flamsteed"/>
<link wikidata="None" start="2929" end="2944" title="Nicolas-Louis de Lacaille#Main works" label="Lacaille listed"/>
<link wikidata="Q604428" start="3219" end="3229" title="OSIRIS-REx" label="OSIRIS-REx"/>
