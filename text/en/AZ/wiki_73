<link wikidata="Q34713" start="3941" end="3949" title="Salzburg" label="Salzburg"/>
<link wikidata="Q2546006" start="3956" end="3975" title="Keys of Heaven" label="keys of Saint Peter"/>
<link wikidata="Q683488" start="4001" end="4024" title="Berchtesgaden Provostry" label="Berchtesgaden Provostry"/>
<link wikidata="Q980" start="4092" end="4099" title="Bavaria" label="Bavaria"/>
