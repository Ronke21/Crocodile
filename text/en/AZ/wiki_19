<link wikidata="Q713623" start="2941" end="2946" title="clade" label="clade"/>
<link wikidata="None" start="3105" end="3117" title="Murtoilestes" label="Murtoilestes"/>
<link wikidata="None" start="3134" end="3148" title="Prokennalestes" label="Prokennalestes"/>
<link wikidata="Q785301" start="4608" end="4628" title="Evolution of mammals" label="Evolution of mammals"/>
<link wikidata="Q16894713" start="4637" end="4645" title="Juramaia" label="Juramaia"/>
