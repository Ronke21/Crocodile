<link wikidata="Q22451931" start="3805" end="3823" title="Cathedral Mountain (Yoho)" label="Cathedral Mountain"/>
<link wikidata="Q2980956" start="3826" end="3839" title="The President (mountain)" label="The President"/>
<link wikidata="Q22427003" start="3842" end="3857" title="Odaray Mountain" label="Odaray Mountain"/>
