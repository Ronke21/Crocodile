<link wikidata="Q131181" start="114" end="130" title="Multituberculata" label="Multituberculata"/>
<link wikidata="Q603912" start="160" end="171" title="Cimolodonta" label="Cimolodonta"/>
<link wikidata="Q5285191" start="207" end="226" title="Djadochtatherioidea" label="Djadochtatherioidea"/>
<link wikidata="Q218302" start="276" end="299" title="Zofia Kielan-Jaworowska" label="Zofia Kielan-Jaworowska"/>
<link wikidata="Q677164" start="350" end="359" title="Flaming Cliffs" label="Bayn Dzak"/>
