<link wikidata="Q605140" start="244" end="248" title="falx" label="falx"/>
<link wikidata="Q1747689" start="278" end="290" title="ancient Rome" label="ancient Rome"/>
<link wikidata="Q203788" start="622" end="626" title="chariot" label="biga"/>
<link wikidata="Q829120" start="724" end="729" title="dux" label="duces"/>
<link wikidata="Q216541" start="789" end="798" title="chauffeur" label="chauffeur"/>
