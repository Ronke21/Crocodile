<link wikidata="Q192624" start="61" end="66" title="meter (poetry)" label="meter"/>
<link wikidata="Q482" start="70" end="74" title="poem" label="poem"/>
<link wikidata="Q285573" start="133" end="138" title="line (poetry)" label="lines"/>
<link wikidata="Q8188" start="279" end="287" title="syllable" label="syllable"/>
<link wikidata="Q2481042" start="378" end="384" title="iambic pentameter" label="iambic"/>
<link wikidata="Q208653" start="429" end="437" title="Trochee" label="trochaic"/>
<link wikidata="Q946219" start="442" end="450" title="Spondee" label="spondaic"/>
<link wikidata="Q3043060" start="455" end="463" title="dactylic pentameter" label="dactylic"/>
