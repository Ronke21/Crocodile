<link wikidata="Q189998" start="100" end="108" title="flagellum" label="flagella"/>
<link wikidata="None" start="295" end="304" title="desmokont" label="desmokont"/>
<link wikidata="None" start="325" end="333" title="dinokont" label="dinokont"/>
<link wikidata="Q855871" start="415" end="431" title="Prorocentrales" label="desmoflagellates"/>
