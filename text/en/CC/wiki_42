<link wikidata="Q4681307" start="45" end="74" title="Addition-chain exponentiation" label="addition-chain exponentiation"/>
<link wikidata="Q33456" start="90" end="104" title="exponentiation" label="exponentiation"/>
<link wikidata="Q12503" start="119" end="126" title="integer" label="integer"/>
<link wikidata="Q4681316" start="227" end="241" title="addition chain" label="addition chain"/>
