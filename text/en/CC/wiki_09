<link wikidata="Q326124" start="119" end="128" title="directive (European Union)" label="directive"/>
<link wikidata="Q240141" start="148" end="158" title="regulation (European Union)" label="regulation"/>
<link wikidata="Q664183" start="491" end="510" title="consumer protection" label="consumer protection"/>
