<link wikidata="Q7176" start="143" end="157" title="Naguib Mahfouz" label="Naguib Mahfouz"/>
<link wikidata="Q13955" start="190" end="196" title="Arabic language" label="Arabic"/>
<link wikidata="Q1860" start="202" end="209" title="English language" label="English"/>
<link wikidata="Q5259858" start="221" end="241" title="Denys Johnson-Davies" label="Denys Johnson-Davies"/>
<link wikidata="Q1251563" start="259" end="268" title="Doubleday (publisher)" label="Doubleday"/>
