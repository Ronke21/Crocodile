<link wikidata="Q170931" start="126" end="131" title="kendo" label="kendo"/>
<link wikidata="Q589009" start="432" end="436" title="kiai" label="kiai"/>
<link wikidata="Q622496" start="653" end="659" title="shinai" label="shinai"/>
<link wikidata="Q371798" start="807" end="812" title="tsuba" label="tsuba"/>
