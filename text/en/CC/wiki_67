<link wikidata="Q1790662" start="4161" end="4178" title="William Brickly Stokes" label="William B. Stokes"/>
<link wikidata="Q7503357" start="5054" end="5062" title="Poll tax (United States)" label="poll tax"/>
<link wikidata="Q502342" start="5717" end="5731" title="Hamblen County, Tennessee" label="Hamblen County"/>
<link wikidata="Q494768" start="5775" end="5784" title="Jefferson County, Tennessee" label="Jefferson"/>
<link wikidata="Q502400" start="5790" end="5797" title="Hawkins County, Tennessee" label="Hawkins"/>
<link wikidata="Q2330995" start="6099" end="6109" title="Morristown, Tennessee" label="Morristown"/>
