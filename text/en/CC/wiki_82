<link wikidata="Q697534" start="137" end="171" title="Hofbräuhaus" label="Staatliches Hofbräuhaus in München"/>
<link wikidata="Q5876532" start="226" end="242" title="Hofbräu-Festzelt" label="Hofbräu-Festzelt"/>
<link wikidata="Q152971" start="298" end="319" title="Hofbräuhaus am Platzl" label="Hofbräuhaus am Platzl"/>
<link wikidata="Q2359534" start="322" end="341" title="Stuttgarter Hofbräu" label="Stuttgarter Hofbräu"/>
<link wikidata="Q870325" start="383" end="401" title="Würzburger Hofbräu" label="Würzburger Hofbräu"/>
