<link wikidata="Q235319" start="3134" end="3148" title="Beijing Subway" label="Beijing Subway"/>
<link wikidata="Q4881181" start="3175" end="3186" title="Beijing Bus" label="Beijing Bus"/>
<link wikidata="Q1042167" start="3248" end="3277" title="Chinese Ministry of Education" label="Chinese Ministry of Education"/>
<link wikidata="None" start="3786" end="3805" title="Five-Year Plans of China#Eleventh Five-Year Guideline.2C 2006-2010" label="11th Five Year Plan"/>
<link wikidata="Q8567" start="4043" end="4061" title="2008 Olympic Games" label="2008 Olympic Games"/>
