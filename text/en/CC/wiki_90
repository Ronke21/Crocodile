<link wikidata="Q38789" start="1800" end="1820" title="Battle of Stalingrad" label="Battle of Stalingrad"/>
<link wikidata="Q41749" start="2502" end="2517" title="Joseph Goebbels" label="Joseph Goebbels"/>
