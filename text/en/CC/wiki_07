<link wikidata="Q34" start="93" end="99" title="Sweden" label="Sweden"/>
<link wikidata="Q8134" start="116" end="124" title="Economics" label="economic"/>
<link wikidata="Q188094" start="280" end="289" title="economist" label="economist"/>
</links>
