<link wikidata="Q118093" start="79" end="91" title="Transmission (telecommunications)" label="transmission"/>
<link wikidata="Q412438" start="121" end="138" title="image compression" label="image compression"/>
<link wikidata="Q4751065" start="276" end="295" title="analog transmission" label="analog transmission"/>
<link wikidata="Q11306265" start="342" end="347" title="signal noise" label="noise"/>
