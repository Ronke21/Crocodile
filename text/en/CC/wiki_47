<link wikidata="Q1310203" start="3812" end="3834" title="independent suspension" label="independent suspension"/>
<link wikidata="Q2301895" start="4789" end="4809" title="Alfa Romeo Giulia TZ" label="Alfa Romeo Giulia TZ"/>
