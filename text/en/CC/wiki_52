<link wikidata="Q2446733" start="2727" end="2753" title="Jane&amp;#x27;s Defence Weekly" label="Jane&amp;#x27;s Defence Weekly"/>
<link wikidata="Q10305195" start="2870" end="2900" title="Jane&amp;#x27;s Combat Simulations" label="Jane&amp;#x27;s Combat Simulations"/>
