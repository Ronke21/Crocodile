<link wikidata="Q273623" start="86" end="99" title="radio station" label="radio station"/>
<link wikidata="Q5137431" start="181" end="193" title="Co-channel interference" label="Interference"/>
<link wikidata="Q769152" start="269" end="288" title="directional antenna" label="directional antenna"/>
<link wikidata="Q560348" start="464" end="484" title="single sideband" label="single sideband mode"/>
