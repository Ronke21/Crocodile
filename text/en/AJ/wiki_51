<link wikidata="Q2267362" start="4568" end="4584" title="Bounded function" label="Bounded function"/>
<link wikidata="Q6664291" start="4586" end="4603" title="Local boundedness" label="Local boundedness"/>
<link wikidata="Q24175351" start="4605" end="4617" title="Order theory" label="Order theory"/>
