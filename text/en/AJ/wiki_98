<link wikidata="Q79913" start="48" end="51" title="Non-governmental organization" label="NGO"/>
<link wikidata="None" start="87" end="102" title="phytoenergetics" label="phytoenergetics"/>
<link wikidata="Q213" start="139" end="153" title="Czech Republic" label="Czech Republic"/>
<link wikidata="Q18537" start="252" end="259" title="biomass" label="biomass"/>
<link wikidata="Q5412312" start="310" end="338" title="European Biomass Association" label="European Biomass Association"/>
</links>
