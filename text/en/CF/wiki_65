<link wikidata="Q3209069" start="75" end="87" title="social dance" label="social dance"/>
<link wikidata="Q131454" start="103" end="122" title="Library of Congress" label="Library of Congress"/>
<link wikidata="Q2615945" start="739" end="757" title="Thomas Jefferson Building" label="Jefferson Building"/>
</links>
