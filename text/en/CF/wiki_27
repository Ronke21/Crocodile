<link wikidata="Q4022" start="149" end="154" title="river" label="river"/>
<link wikidata="Q584" start="155" end="160" title="Rhine" label="Rhine"/>
<link wikidata="Q568819" start="354" end="369" title="Bijlands Kanaal" label="Bijlands Kanaal"/>
<link wikidata="Q1918938" start="448" end="457" title="Pannerden" label="Pannerden"/>
