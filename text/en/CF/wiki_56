<link wikidata="Q185029" start="47" end="67" title="first-person shooter" label="first-person shooter"/>
<link wikidata="Q189784" start="1558" end="1562" title="DOOM" label="DOOM"/>
<link wikidata="Q193581" start="1722" end="1733" title="Half-Life 2" label="Half-Life 2"/>
<link wikidata="Q1347898" start="1846" end="1849" title="Lag (video games)" label="lag"/>
