<link wikidata="Q40358" start="4755" end="4806" title="American Association for the Advancement of Science" label="American Association for the Advancement of Science"/>
<link wikidata="Q49120" start="4840" end="4853" title="Tufts College" label="Tufts College"/>
<link wikidata="Q49205" start="4897" end="4914" title="Wellesley College" label="Wellesley College"/>
