<link wikidata="Q7021002" start="3254" end="3269" title="Next Generation (magazine)" label="Next Generation"/>
<link wikidata="Q1965664" start="3963" end="3989" title="Zero the Kamikaze Squirrel" label="Zero the Kamikaze Squirrel"/>
