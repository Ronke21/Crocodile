<link wikidata="Q67972035" start="134" end="147" title="laser printer" label="laser printer"/>
<link wikidata="Q95474715" start="152" end="166" title="inkjet printer" label="inkjet printer"/>
<link wikidata="Q876049" start="230" end="234" title="Very-large-scale integration" label="VLSI"/>
<link wikidata="Q622938" start="235" end="251" title="photolithography" label="photolithography"/>
