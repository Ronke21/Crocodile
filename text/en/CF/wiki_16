<link wikidata="Q166902" start="108" end="121" title="primary color" label="primary color"/>
<link wikidata="Q1421299" start="518" end="544" title="Grey component replacement" label="grey component replacement"/>
<link wikidata="Q355852" start="573" end="592" title="under color removal" label="under color removal"/>
<link wikidata="Q166432" start="1469" end="1485" title="CMYK color model" label="CMYK color model"/>
</links>
