<link wikidata="Q7883229" start="56" end="59" title="Under color addition" label="UCA"/>
<link wikidata="Q355852" start="184" end="203" title="under color removal" label="under color removal"/>
<link wikidata="Q1409234" start="1052" end="1062" title="rich black" label="rich black"/>
