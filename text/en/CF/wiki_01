<link wikidata="Q57052" start="169" end="176" title="Jiangxi" label="Jiangxi"/>
<link wikidata="Q7183" start="245" end="256" title="Qin dynasty" label="Qin dynasty"/>
<link wikidata="Q5413" start="380" end="387" title="Yangtze" label="Yangtze"/>
<link wikidata="Q1023893" start="398" end="404" title="Nanhai District" label="Nanhai"/>
<link wikidata="Q9683" start="442" end="446" title="Tang dynasty" label="Tang"/>
<link wikidata="Q712065" start="542" end="555" title="Zhang Jiuling" label="Zhang Jiuling"/>
