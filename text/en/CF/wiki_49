<link wikidata="Q40157" start="58" end="62" title="lava" label="lava"/>
<link wikidata="Q621022" start="223" end="228" title="Atlas (crater)" label="Atlas"/>
<link wikidata="Q1070916" start="247" end="257" title="Chevallier (crater)" label="Chevallier"/>
