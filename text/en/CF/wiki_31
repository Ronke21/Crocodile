<link wikidata="Q9143" start="61" end="81" title="programming language" label="programming language"/>
<link wikidata="Q177646" start="85" end="93" title="Subset" label="superset"/>
<link wikidata="Q1209759" start="97" end="105" title="ALGOL 60" label="ALGOL 60"/>
<link wikidata="Q4117397" start="169" end="196" title="system programming language" label="system programming language"/>
<link wikidata="Q846651" start="302" end="317" title="multiprocessing" label="multiprocessing"/>
