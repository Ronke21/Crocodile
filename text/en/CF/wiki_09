<link wikidata="Q37930" start="279" end="284" title="Postage stamp" label="stamp"/>
<link wikidata="Q3311985" start="802" end="810" title="Guernsey" label="Guernsey"/>
<link wikidata="Q2238977" start="838" end="871" title="occupation of the Channel Islands" label="occupation of the Channel Islands"/>
<link wikidata="Q362" start="879" end="891" title="World War II" label="World War II"/>
