<link wikidata="None" start="5293" end="5314" title="Abu Abdul Halim Dalak" label="Abu Abdul Halim Dalak"/>
<link wikidata="Q335523" start="5381" end="5393" title="Abu Zubaydah" label="Abu Zubaydah"/>
<link wikidata="Q16200071" start="5438" end="5450" title="Noor al-Deen (detainee)" label="Noor al-Deen"/>
