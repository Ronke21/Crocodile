<link wikidata="Q665489" start="2645" end="2660" title="Royal Tunbridge Wells" label="Tunbridge Wells"/>
<link wikidata="Q57106" start="3173" end="3192" title="Heinrich Schliemann" label="Heinrich Schliemann"/>
<link wikidata="Q60239" start="3194" end="3210" title="Wilhelm Dorpfeld" label="Wilhelm Dorpfeld"/>
<link wikidata="Q552469" start="3216" end="3227" title="Carl Blegen" label="Carl Blegen"/>
<link wikidata="Q22647" start="3267" end="3271" title="Troy" label="Troy"/>
<link wikidata="Q974205" start="3693" end="3705" title="Xerxes Canal" label="Xerxes Canal"/>
