<link wikidata="Q1675427" start="37" end="55" title="Metsä Board" label="M-real Corporation"/>
<link wikidata="Q14934005" start="94" end="104" title="paperboard" label="paperboard"/>
<link wikidata="Q1925962" start="301" end="312" title="Metsä Group" label="Metsä Group"/>
