<link wikidata="Q783565" start="5478" end="5491" title="Kitáb-i-Badíʻ" label="Kitáb-i-Badíʻ"/>
<link wikidata="Q2348926" start="5705" end="5718" title="Ásiyih Khánum" label="Ásiyih Khánum"/>
<link wikidata="Q3867232" start="5751" end="5765" title="Munirih Khánum" label="Munirih Khánum"/>
