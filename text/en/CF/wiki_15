<link wikidata="Q15028" start="91" end="94" title="International Organization for Standardization" label="ISO"/>
<link wikidata="Q731128" start="120" end="125" title="X.500" label="X.500"/>
<link wikidata="Q756230" start="126" end="143" title="directory service" label="directory service"/>
