<link wikidata="Q5891" start="112" end="124" title="philosophers" label="philosophers"/>
<link wikidata="Q42240" start="141" end="152" title="researchers" label="researchers"/>
<link wikidata="Q186285" start="160" end="184" title="University of Copenhagen" label="University of Copenhagen"/>
