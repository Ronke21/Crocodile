<link wikidata="Q313424" start="4037" end="4046" title="Ian Smith" label="Ian Smith"/>
<link wikidata="Q2925753" start="4452" end="4479" title="British South Africa Police" label="British South Africa Police"/>
<link wikidata="Q3921" start="5254" end="5260" title="Harare" label="Harare"/>
