<link wikidata="Q8261" start="104" end="109" title="novel" label="novel"/>
<link wikidata="Q762195" start="128" end="135" title="Esquire (magazine)" label="Esquire"/>
<link wikidata="Q345612" start="143" end="155" title="Harold Bloom" label="Harold Bloom"/>
<link wikidata="Q27474" start="261" end="286" title="New Directions Publishing" label="New Directions Publishing"/>
<link wikidata="Q484495" start="394" end="406" title="alliteration" label="alliterative"/>
<link wikidata="Q836165" start="419" end="427" title="lipogram" label="lipogram"/>
<link wikidata="Q5917710" start="1706" end="1722" title="How German Is It" label="How German Is It"/>
