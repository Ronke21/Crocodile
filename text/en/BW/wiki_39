<link wikidata="Q36602" start="45" end="55" title="infraorder (biology)" label="infraorder"/>
<link wikidata="Q240514" start="56" end="67" title="Muscomorpha" label="Muscomorpha"/>
<link wikidata="Q170595" start="222" end="230" title="puparium" label="puparium"/>
