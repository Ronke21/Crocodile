<link wikidata="None" start="224" end="250" title="Bark River (Lake Superior)" label="Bark River (Lake Superior)"/>
<link wikidata="Q2327057" start="254" end="272" title="List of rivers of Wisconsin" label="river in Wisconsin"/>
