<link wikidata="None" start="751" end="761" title="Transmission Control Protocol#TCP segment structure" label="TCP header"/>
<link wikidata="Q8795" start="884" end="901" title="Internet Protocol" label="Internet Protocol"/>
<link wikidata="Q1406" start="1106" end="1123" title="Microsoft Windows" label="Microsoft Windows"/>
