<link wikidata="Q81653535" start="4069" end="4089" title="594913 ꞌAylóꞌchaxnim" label="594913 ꞌAylóꞌchaxnim"/>
<link wikidata="Q43401707" start="4137" end="4162" title="Zwicky Transient Facility" label="Zwicky Transient Facility"/>
<link wikidata="Q308" start="4394" end="4401" title="Mercury (planet)" label="Mercury"/>
