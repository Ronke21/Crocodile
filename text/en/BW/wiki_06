<link wikidata="Q7566" start="353" end="359" title="parent" label="parent"/>
<link wikidata="Q1080497" start="385" end="395" title="left brain" label="left brain"/>
<link wikidata="Q7569" start="439" end="444" title="child" label="child"/>
