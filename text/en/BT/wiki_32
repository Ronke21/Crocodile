<link wikidata="Q2315928" start="152" end="163" title="Ice hockey rink" label="hockey rink"/>
<link wikidata="Q1215892" start="530" end="533" title="National Hockey League" label="NHL"/>
<link wikidata="Q5146601" start="750" end="768" title="college ice hockey" label="college ice hockey"/>
<link wikidata="Q793654" start="1023" end="1044" title="Checking (ice hockey)" label="Checking (ice hockey)"/>
</links>
