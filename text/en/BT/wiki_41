<link wikidata="Q311198" start="127" end="131" title="real vs. nominal in economics" label="real"/>
<link wikidata="Q12638" start="132" end="154" title="gross domestic product" label="gross domestic product"/>
<link wikidata="Q46607" start="482" end="493" title="free market" label="free market"/>
<link wikidata="Q3218843" start="535" end="549" title="price controls" label="price controls"/>
<link wikidata="Q35865" start="552" end="561" title="inflation" label="inflation"/>
<link wikidata="Q4402708" start="583" end="589" title="demand" label="demand"/>
