<link wikidata="Q956883" start="26" end="40" title="carnatic music" label="carnatic music"/>
<link wikidata="Q6438562" start="90" end="96" title="Krithi" label="Krithi"/>
<link wikidata="Q7282802" start="229" end="249" title="Ragam Thanam Pallavi" label="Ragam Thanam Pallavi"/>
