<link wikidata="Q206" start="5365" end="5379" title="Stephen Harper" label="Stephen Harper"/>
<link wikidata="Q55138770" start="5419" end="5432" title="Ontario Proud" label="Ontario Proud"/>
<link wikidata="Q849095" start="5519" end="5544" title="Canadian federal election" label="Canadian federal election"/>
