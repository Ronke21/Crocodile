<link wikidata="Q12252367" start="5734" end="5749" title="Atherosclerosis" label="Atherosclerosis"/>
<link wikidata="Q1209340" start="6054" end="6080" title="2007 Southeast Asian Games" label="2007 Southeast Asian Games"/>
<link wikidata="Q42707" start="6111" end="6129" title="List of cat breeds" label="List of cat breeds"/>
<link wikidata="Q42604" start="6132" end="6143" title="Siamese cat" label="Siamese cat"/>
<link wikidata="Q42732" start="6146" end="6154" title="Thai cat" label="Thai cat"/>
