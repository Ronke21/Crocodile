<link wikidata="Q1735729" start="350" end="360" title="catechesis" label="catechesis"/>
<link wikidata="Q51646" start="552" end="572" title="Apostles&amp;#x27; Creed" label="Apostles&amp;#x27; Creed"/>
<link wikidata="Q23393" start="667" end="685" title="Lord&amp;#x27;s Prayer" label="Lord&amp;#x27;s Prayer"/>
</links>
