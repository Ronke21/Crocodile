<link wikidata="Q1063239" start="16963" end="16979" title="political entity" label="political entity"/>
<link wikidata="Q1071027" start="17026" end="17036" title="prosoponym" label="prosoponym"/>
<link wikidata="Q61002" start="17092" end="17101" title="pseudonym" label="pseudonym"/>
<link wikidata="Q127843" start="17171" end="17179" title="pen name" label="pen name"/>
<link wikidata="Q1130239" start="17182" end="17190" title="retronym" label="retronym"/>
<link wikidata="Q42106" start="17542" end="17549" title="synonym" label="synonym"/>
<link wikidata="Q1040689" start="18035" end="18053" title="synonym (taxonomy)" label="synonym (taxonomy)"/>
