<link wikidata="Q349291" start="5784" end="5790" title="Apries" label="Apries"/>
<link wikidata="Q48685" start="5816" end="5821" title="Kingdom of Judah" label="Judah"/>
<link wikidata="Q223644" start="5864" end="5876" title="First Temple" label="First Temple"/>
<link wikidata="Q5684" start="5927" end="5934" title="Babylon" label="Babylon"/>
<link wikidata="Q20426837" start="5940" end="5964" title="Nebuchadnezzar Chronicle" label="Nebuchadnezzar Chronicle"/>
