<link wikidata="Q16225552" start="11997" end="12015" title="Ava Leavell Haymon" label="Ava Leavell Haymon"/>
<link wikidata="Q6308299" start="12017" end="12027" title="Julie Kane" label="Julie Kane"/>
<link wikidata="Q7173377" start="12029" end="12041" title="Peter Cooley" label="Peter Cooley"/>
<link wikidata="Q54611293" start="12043" end="12060" title="John Warner Smith" label="John Warner Smith"/>
<link wikidata="Q87485040" start="12065" end="12080" title="Mona Lisa Saloy" label="Mona Lisa Saloy"/>
<link wikidata="Q7983963" start="12243" end="12256" title="Wesley McNair" label="Wesley McNair"/>
<link wikidata="Q4862922" start="12258" end="12271" title="Baron Wormser" label="Baron Wormser"/>
<link wikidata="Q4898329" start="12277" end="12288" title="Betsy Sholl" label="Betsy Sholl"/>
