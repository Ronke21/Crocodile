<link wikidata="Q430639" start="27" end="45" title="Sarah Josepha Hale" label="Sarah Josepha Hale"/>
<link wikidata="Q44077151" start="87" end="103" title="SS Sarah J. Hale" label="SS Sarah J. Hale"/>
