<link wikidata="Q5295" start="224" end="244" title="random-access memory" label="random-access memory"/>
<link wikidata="Q1297822" start="360" end="369" title="copyright" label="copyright"/>
<link wikidata="Q11268" start="422" end="444" title="United States Congress" label="United States Congress"/>
