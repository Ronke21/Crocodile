<link wikidata="Q2643251" start="380" end="387" title="Chemical synapse" label="synapse"/>
<link wikidata="Q1207387" start="597" end="630" title="excitatory postsynaptic potential" label="excitatory postsynaptic potential"/>
<link wikidata="Q3089249" start="768" end="789" title="equilibrium potential" label="equilibrium potential"/>
<link wikidata="Q411094" start="855" end="868" title="ibotenic acid" label="ibotenic acid"/>
<link wikidata="Q112523975" start="885" end="902" title="Povl Krogsgaard-Larsen" label="Krogsgaard-Larsen"/>
<link wikidata="Q2495337" start="1028" end="1036" title="Ampakine" label="Ampakine"/>
</links>
