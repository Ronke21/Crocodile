<link wikidata="Q162657" start="175" end="191" title="neurotransmitter" label="neurotransmitter"/>
<link wikidata="Q120785193" start="192" end="201" title="Glutamate (neurotransmitter)" label="glutamate"/>
<link wikidata="Q390239" start="304" end="315" title="kainic acid" label="kainic acid"/>
