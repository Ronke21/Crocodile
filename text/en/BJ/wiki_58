<link wikidata="Q9143" start="325" end="345" title="programming language" label="programming language"/>
<link wikidata="Q1437428" start="388" end="397" title="Formal semantics of programming languages" label="semantics"/>
<link wikidata="Q296187" start="561" end="564" title="APL programming language" label="APL"/>
<link wikidata="Q3737844" start="606" end="610" title="Binary function" label="dyad"/>
<link wikidata="Q656784" start="1913" end="1934" title="Rank (linear algebra)" label="Rank (linear algebra)"/>
<link wikidata="Q44337" start="1987" end="1995" title="matrix (mathematics)" label="matrices"/>
<link wikidata="Q7293199" start="1997" end="2026" title="Rank (J programming language)" label="Rank (J programming language)"/>
