<link wikidata="Q170130" start="411" end="425" title="vector graphics" label="vector graphic"/>
<link wikidata="Q2449569" start="497" end="509" title="transparency (graphic)" label="transparency"/>
<link wikidata="Q1752634" start="578" end="584" title="Xara Xtreme" label="Xara X"/>
