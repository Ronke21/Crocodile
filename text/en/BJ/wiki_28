<link wikidata="Q254215" start="16824" end="16841" title="Rosemary Sutcliff" label="Rosemary Sutcliff"/>
<link wikidata="Q6152466" start="17236" end="17245" title="Jane Lane (author)" label="Jane Lane"/>
