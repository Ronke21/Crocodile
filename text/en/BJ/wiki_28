<link wikidata="Q8047414" start="22984" end="23006" title="Yale Repertory Theatre" label="Yale Repertory Theatre"/>
<link wikidata="Q217573" start="23033" end="23045" title="Dianne Wiest" label="Dianne Wiest"/>
