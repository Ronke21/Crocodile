<link wikidata="Q2333568" start="47" end="67" title="Optimal substructure" label="optimal substructure"/>
<link wikidata="Q504353" start="254" end="270" title="greedy algorithm" label="greedy algorithm"/>
<link wikidata="Q7113766" start="433" end="456" title="overlapping subproblems" label="overlapping subproblems"/>
<link wikidata="Q671298" start="466" end="484" title="Divide and conquer algorithm" label="divide-and-conquer"/>
<link wikidata="Q380679" start="496" end="515" title="dynamic programming" label="dynamic programming"/>
