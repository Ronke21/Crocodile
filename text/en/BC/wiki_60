<link wikidata="Q193627" start="171" end="189" title="logical positivist" label="logical positivist"/>
<link wikidata="Q58854" start="409" end="426" title="literary analysis" label="literary analysis"/>
<link wikidata="Q83368" start="441" end="451" title="Empiricism" label="Empiricism"/>
<link wikidata="Q220888" start="453" end="467" title="Falsifiability" label="Falsifiability"/>
