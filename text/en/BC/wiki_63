<link wikidata="Q16988917" start="16827" end="16835" title="Same-sex marriage in Kentucky" label="Kentucky"/>
<link wikidata="Q1612" start="17371" end="17379" title="Arkansas" label="Arkansas"/>
<link wikidata="Q812" start="17381" end="17388" title="Florida" label="Florida"/>
<link wikidata="Q1415" start="17390" end="17397" title="Indiana" label="Indiana"/>
