<link wikidata="Q1330136" start="8212" end="8237" title="Second Battle of Fallujah" label="Second Battle of Fallujah"/>
<link wikidata="Q4871794" start="8292" end="8307" title="Battle of Mosul (2004)" label="Battle of Mosul"/>
<link wikidata="Q4602814" start="8339" end="8380" title="2004 Forward Operating Base Marez bombing" label="2004 Forward Operating Base Marez bombing"/>
<link wikidata="Q2297120" start="8453" end="8479" title="January 2005 Iraqi legislative election" label="Iraqi legislative election"/>
<link wikidata="Q1377438" start="8490" end="8511" title="United Iraqi Alliance" label="United Iraqi Alliance"/>
<link wikidata="Q1419636" start="8549" end="8565" title="Kurdish Alliance" label="Kurdish Alliance"/>
