<link wikidata="Q1046867" start="25578" end="25588" title="Chrysler PT Cruiser" label="PT Cruiser"/>
<link wikidata="Q1088651" start="26652" end="26667" title="Plymouth Breeze" label="Plymouth Breeze"/>
