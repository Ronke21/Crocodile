<link wikidata="Q126818" start="33" end="53" title="dimensionless number" label="dimensionless number"/>
<link wikidata="Q910483" start="75" end="87" title="viscoelastic" label="viscoelastic"/>
<link wikidata="Q94404" start="113" end="129" title="Karl Weissenberg" label="Karl Weissenberg"/>
