<link wikidata="Q37785702" start="208" end="232" title="Nasikabatrachus bhupathi" label="Nasikabatrachus bhupathi"/>
<link wikidata="Q311152" start="249" end="277" title="Nasikabatrachus sahyadrensis" label="Nasikabatrachus sahyadrensis"/>
