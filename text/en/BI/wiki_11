<link wikidata="Q9592" start="7256" end="7264" title="Catholic" label="Catholic"/>
<link wikidata="Q600751" start="7667" end="7678" title="prosecution" label="prosecution"/>
<link wikidata="Q6143445" start="8229" end="8244" title="James Spigelman" label="James Spigelman"/>
<link wikidata="Q7238079" start="8896" end="8907" title="Prairiewood, New South Wales" label="Prairiewood"/>
