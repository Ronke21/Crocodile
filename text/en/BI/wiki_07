<link wikidata="Q172491" start="1867" end="1878" title="data mining" label="data mining"/>
<link wikidata="Q1172284" start="2693" end="2704" title="data set" label="set of data"/>
<link wikidata="Q494735" start="2861" end="2881" title="statistical software" label="statistical software"/>
