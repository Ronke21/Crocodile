<link wikidata="Q7335580" start="4730" end="4747" title="Ripe Banana Skins" label="Ripe Banana Skins"/>
<link wikidata="Q8341" start="4978" end="4982" title="jazz" label="jazz"/>
<link wikidata="Q8341" start="5006" end="5010" title="Jazz" label="Jazz"/>
<link wikidata="Q399102" start="5612" end="5635" title="Agustín Barrios Mangoré" label="Agustín Barrios Mangoré"/>
<link wikidata="Q1395857" start="5637" end="5657" title="José Asunción Flores" label="José Asunción Flores"/>
<link wikidata="Q8247045" start="5659" end="5670" title="Berta Rojas" label="Berta Rojas"/>
