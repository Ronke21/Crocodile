<link wikidata="Q191360" start="121" end="131" title="foundation (architecture)" label="foundation"/>
<link wikidata="Q22657" start="187" end="195" title="concrete" label="concrete"/>
<link wikidata="Q1470555" start="1037" end="1047" title="frost line" label="frost line"/>
<link wikidata="Q56875480" start="1119" end="1132" title="Tile drainage" label="Drainage tile"/>
</links>
