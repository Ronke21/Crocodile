<link wikidata="Q23691" start="88" end="103" title="national anthem" label="national anthem"/>
<link wikidata="Q209951" start="122" end="134" title="Nauru Bwiema" label="Nauru Bwiema"/>
<link wikidata="Q2481188" start="170" end="186" title="Margaret Hendrie" label="Margaret Hendrie"/>
<link wikidata="Q2564957" start="204" end="224" title="Laurence Henry Hicks" label="Laurence Henry Hicks"/>
<link wikidata="Q563297" start="279" end="289" title="Baron Waqa" label="Baron Waqa"/>
