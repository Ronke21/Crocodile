<link wikidata="Q1743272" start="206" end="213" title="Kircher (crater)" label="Kircher"/>
<link wikidata="Q227650" start="260" end="268" title="Zucchius (crater)" label="Zucchius"/>
<link wikidata="Q773075" start="345" end="351" title="Bailly (crater)" label="Bailly"/>
