<link wikidata="Q1135914" start="60" end="72" title="Event-driven programming" label="event-driven"/>
<link wikidata="Q9143" start="73" end="93" title="programming language" label="programming language"/>
<link wikidata="Q1934217" start="129" end="135" title="TinyOS" label="TinyOS"/>
<link wikidata="Q2713600" start="159" end="180" title="operating environment" label="operating environment"/>
<link wikidata="Q336038" start="237" end="260" title="wireless sensor network" label="wireless sensor network"/>
<link wikidata="Q15777" start="300" end="322" title="C (programming language)" label="C programming language"/>
</links>
