<link wikidata="Q1754768" start="636" end="663" title="Cost-effectiveness analysis" label="Cost-effectiveness analysis"/>
<link wikidata="None" start="666" end="687" title="Scoring and weighting" label="Scoring and weighting"/>
</links>
