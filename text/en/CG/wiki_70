<link wikidata="Q1003662" start="94" end="103" title="Dahlonega, Georgia" label="Dahlonega"/>
<link wikidata="Q12148490" start="149" end="162" title="placer mining" label="placer mining"/>
<link wikidata="Q3453701" start="232" end="249" title="Georgia Gold Rush" label="Georgia Gold Rush"/>
