<link wikidata="Q3786866" start="264" end="271" title="HMSHost" label="HMSHost"/>
<link wikidata="Q7913552" start="540" end="578" title="Van Noy Railway News and Hotel Company" label="Van Noy Railway News and Hotel Company"/>
<link wikidata="Q3786866" start="843" end="850" title="HMSHost" label="HMSHost"/>
<link wikidata="Q786790" start="862" end="878" title="Autogrill" label="Autogrill S.p.A."/>
</links>
