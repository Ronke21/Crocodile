<link wikidata="Q474563" start="8962" end="8982" title="Teyler&amp;#x27;s Museum" label="Teyler&amp;#x27;s Museum"/>
<link wikidata="Q78990" start="8999" end="9023" title="Joseph Franz von Jacquin" label="Joseph Franz von Jacquin"/>
<link wikidata="Q334364" start="9078" end="9089" title="John Latham (ornithologist)" label="John Latham"/>
<link wikidata="Q545123" start="9208" end="9222" title="Thomas Pennant" label="Thomas Pennant"/>
