<link wikidata="Q123452" start="146" end="153" title="Endemism" label="endemic"/>
<link wikidata="Q36117" start="157" end="163" title="Borneo" label="Borneo"/>
<link wikidata="None" start="208" end="237" title="Paraphalaenopsis × thorntonii" label="Paraphalaenopsis × thorntonii"/>
