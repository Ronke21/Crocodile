<link wikidata="Q505226" start="29" end="53" title="M107 Long Range Sniper Rifle" label="XM107/M107 Barrett rifle"/>
<link wikidata="Q4353870" start="92" end="109" title="M107 (projectile)" label="M107 (projectile)"/>
<link wikidata="Q1752659" start="164" end="187" title="M107 self-propelled gun" label="M107 self-propelled gun"/>
<link wikidata="Q297994" start="228" end="232" title="M107" label="M107"/>
