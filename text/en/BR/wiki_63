<link wikidata="Q40357" start="1210" end="1230" title="prison" label="imprisoned offenders"/>
<link wikidata="Q860554" start="1338" end="1344" title="Performance metric" label="metric"/>
<link wikidata="Q1420643" start="1733" end="1743" title="recidivism" label="recidivism"/>
</links>
