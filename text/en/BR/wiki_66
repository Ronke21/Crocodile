<link wikidata="Q11034" start="63" end="71" title="printing" label="printing"/>
<link wikidata="Q131790" start="114" end="122" title="adhesive" label="adhesive"/>
<link wikidata="Q146505" start="697" end="704" title="solvent" label="solvent"/>
