<link wikidata="Q76612583" start="3405" end="3419" title="Lode Van Hecke" label="Lode Van Hecke"/>
<link wikidata="Q464162" start="3605" end="3624" title="Mathilda of Tuscany" label="Mathilda of Tuscany"/>
