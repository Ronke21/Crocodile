<link wikidata="Q270978" start="28143" end="28153" title="Karekin II" label="Karekin II"/>
<link wikidata="Q589708" start="28388" end="28399" title="Zoltán Bíró" label="Zoltán Bíró"/>
<link wikidata="Q1953" start="28620" end="28627" title="Yerevan" label="Yerevan"/>
<link wikidata="Q80034" start="28950" end="28967" title="Armenian genocide" label="Armenian genocide"/>
