<link wikidata="Q180341" start="56" end="66" title="methionine" label="methionine"/>
<link wikidata="Q83147" start="76" end="92" title="chemical formula" label="chemical formula"/>
<link wikidata="Q4407" start="94" end="97" title="Methyl" label="CH3"/>
<link wikidata="Q36496" start="126" end="132" title="cation" label="cation"/>
<link wikidata="Q423240" start="214" end="223" title="sulfonium" label="sulfonium"/>
