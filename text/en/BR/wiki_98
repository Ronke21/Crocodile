<link wikidata="Q180095" start="123" end="130" title="tyrant" label="tyranny"/>
<link wikidata="Q313467" start="134" end="141" title="Hippias (son of Pisistratus)" label="Hippias"/>
<link wikidata="Q207634" start="225" end="236" title="Cleisthenes" label="Cleisthenes"/>
