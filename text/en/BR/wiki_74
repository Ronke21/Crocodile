<link wikidata="Q574" start="3675" end="3685" title="East Timor" label="East Timor"/>
<link wikidata="Q402096" start="3716" end="3724" title="INTERFET" label="INTERFET"/>
<link wikidata="Q16982111" start="3845" end="3870" title="Glenn Defense Marine Asia" label="Glenn Defense Marine Asia"/>
<link wikidata="None" start="4265" end="4291" title="USS Blue Ridge (LCC-19)" label="USS &amp;quot;Blue Ridge&amp;quot;"/>
<link wikidata="Q336332" start="4519" end="4527" title="ship breaking" label="breaking"/>
<link wikidata="Q376749" start="4546" end="4556" title="Chittagong" label="Chittagong"/>
