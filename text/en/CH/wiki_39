<link wikidata="Q5437035" start="48" end="61" title="Fast fracture" label="fast fracture"/>
<link wikidata="Q5051574" start="156" end="176" title="catastrophic failure" label="catastrophic failure"/>
<link wikidata="Q496309" start="274" end="287" title="strain energy" label="strain energy"/>
