<link wikidata="Q1332643" start="159" end="178" title="boundary conditions" label="boundary conditions"/>
<link wikidata="Q190132" start="216" end="225" title="turbulent" label="turbulent"/>
<link wikidata="Q625303" start="468" end="493" title="statistically independent" label="statistically independent"/>
<link wikidata="Q147027" start="547" end="565" title="thought experiment" label="thought experiment"/>
<link wikidata="None" start="703" end="713" title="mixing box" label="mixing box"/>
