<link wikidata="Q1366963" start="4121" end="4135" title="whale watching" label="whale watching"/>
<link wikidata="Q3092895" start="4302" end="4333" title="National Acadian Day" label="fête nationale de l&amp;#x27;Acadie"/>
<link wikidata="Q110502450" start="4940" end="4951" title="Petit Étang, Nova Scotia" label="Petit Étang"/>
<link wikidata="Q3058135" start="5226" end="5248" title="Université Sainte-Anne" label="Université Sainte-Anne"/>
<link wikidata="Q3462611" start="5265" end="5286" title="Saint-Joseph-du-Moine, Nova Scotia" label="Saint-Joseph-du-Moine"/>
