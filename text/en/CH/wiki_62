<link wikidata="Q333761" start="529" end="549" title="translation memory" label="translation memories"/>
<link wikidata="Q79798" start="590" end="609" title="machine translation" label="machine translation"/>
<link wikidata="Q13230" start="701" end="709" title="Ferdinand de Saussure" label="Saussure"/>
<link wikidata="Q4890502" start="1421" end="1435" title="Susan Bassnett" label="Susan Bassnett"/>
<link wikidata="Q716492" start="1977" end="1988" title="Eugene Nida" label="Eugene Nida"/>
