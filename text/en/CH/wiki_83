<link wikidata="Q5735394" start="181" end="196" title="Richmond Palmer" label="Richmond Palmer"/>
<link wikidata="Q15999647" start="237" end="258" title="Herbert Edward Palmer" label="Herbert Edward Palmer"/>
<link wikidata="Q56025076" start="286" end="307" title="Herbert Sidney Palmer" label="Herbert Sidney Palmer"/>
