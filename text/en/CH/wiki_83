<link wikidata="Q11629" start="56" end="63" title="Painting" label="painter"/>
<link wikidata="Q148" start="75" end="80" title="China" label="China"/>
<link wikidata="Q1348851" start="86" end="100" title="Yangxin County, Shandong" label="Yangxin County"/>
