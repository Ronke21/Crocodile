<link wikidata="Q1103620" start="6569" end="6578" title="Clyde Dam" label="Clyde Dam"/>
<link wikidata="Q196232" start="6875" end="6909" title="Friends of the Earth International" label="Friends of the Earth International"/>
<link wikidata="Q5454876" start="6924" end="6937" title="Fisheries Act 1983" label="Fisheries Act"/>
<link wikidata="Q4081089" start="7000" end="7036" title="New Zealand&amp;#x27;s nuclear-free zone" label="New Zealand&amp;#x27;s nuclear-free zone"/>
