<link wikidata="Q36192" start="56" end="64" title="folklore" label="folklore"/>
<link wikidata="Q178638" start="80" end="88" title="Nasrudin" label="Nasrudin"/>
<link wikidata="Q12560" start="141" end="155" title="Ottoman Empire" label="Ottoman Empire"/>
<link wikidata="Q35032" start="326" end="342" title="Eastern Orthodox Church" label="Eastern Orthodox"/>
