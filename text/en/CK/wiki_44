<link wikidata="Q209976" start="125" end="143" title="Filchner Ice Shelf" label="Filchner Ice Shelf"/>
<link wikidata="Q1117065" start="194" end="233" title="Commonwealth Trans-Antarctic Expedition" label="Commonwealth Trans-Antarctic Expedition"/>
</links>
