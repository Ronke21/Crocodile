<link wikidata="Q96372226" start="12658" end="12684" title="António Fernandes de Elvas" label="António Fernandes de Elvas"/>
<link wikidata="Q653884" start="12713" end="12729" title="Spanish Americas" label="Spanish Americas"/>
<link wikidata="Q657461" start="12760" end="12779" title="Cartagena de Indias" label="Cartagena de Indias"/>
