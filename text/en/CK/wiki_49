<link wikidata="Q262386" start="11653" end="11656" title="ORF (broadcaster)" label="ORF"/>
<link wikidata="Q700927" start="11863" end="11885" title="German submarine U-864" label="German submarine U-864"/>
<link wikidata="Q362" start="11889" end="11901" title="World War II" label="World War II"/>
