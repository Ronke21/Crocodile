<link wikidata="Q442288" start="449" end="461" title="Toledo Blade" label="Toledo Blade"/>
<link wikidata="Q270539" start="464" end="484" title="Charles W. Fairbanks" label="Charles W. Fairbanks"/>
<link wikidata="Q33866" start="631" end="649" title="Theodore Roosevelt" label="Theodore Roosevelt"/>
