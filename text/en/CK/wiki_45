<link wikidata="Q2744875" start="129" end="146" title="Siege of Savannah" label="Siege of Savannah"/>
<link wikidata="Q2284279" start="221" end="242" title="Siege of Fort Pulaski" label="Siege of Fort Pulaski"/>
