<link wikidata="Q2993173" start="77" end="88" title="hieromartyr" label="hieromartyr"/>
<link wikidata="Q107401" start="102" end="109" title="Sivas" label="Sebaste"/>
<link wikidata="Q1148125" start="111" end="118" title="Lesser Armenia" label="Armenia"/>
