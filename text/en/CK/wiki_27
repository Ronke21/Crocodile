<link wikidata="Q17012" start="3804" end="3814" title="Guadeloupe" label="Guadeloupe"/>
<link wikidata="Q2575147" start="3817" end="3846" title="Parti Communiste Guadeloupéen" label="Parti Communiste Guadeloupéen"/>
<link wikidata="Q774" start="3883" end="3892" title="Guatemala" label="Guatemala"/>
