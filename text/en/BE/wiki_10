<link wikidata="Q862435" start="9118" end="9124" title="Lockheed EC-121 Warning Star" label="EC-121"/>
<link wikidata="Q11220" start="9235" end="9239" title="United States Navy" label="Navy"/>
<link wikidata="Q862435" start="9445" end="9464" title="EC-121 Warning Star" label="EC-121 Warning Star"/>
