<link wikidata="Q962630" start="119" end="133" title="Upsala Glacier" label="Upsala Glacier"/>
<link wikidata="Q773868" start="159" end="174" title="Upsala, Ontario" label="Upsala, Ontario"/>
<link wikidata="Q2227067" start="198" end="215" title="Upsala, Minnesota" label="Upsala, Minnesota"/>
<link wikidata="Q7899236" start="242" end="258" title="Upsala (mansion)" label="Upsala (mansion)"/>
<link wikidata="Q7899237" start="311" end="325" title="Upsala College" label="Upsala College"/>
<link wikidata="Q7070770" start="383" end="404" title="New Upsala, Wisconsin" label="New Upsala, Wisconsin"/>
<link wikidata="Q443817" start="487" end="511" title="Uppsala (disambiguation)" label="Uppsala (disambiguation)"/>
