<link wikidata="Q148475" start="24016" end="24043" title="Jean-Baptiste-Camille Corot" label="Jean-Baptiste-Camille Corot"/>
<link wikidata="Q34618" start="24045" end="24060" title="Gustave Courbet" label="Gustave Courbet"/>
<link wikidata="Q143357" start="24070" end="24085" title="Barbizon School" label="Barbizon School"/>
<link wikidata="Q40415" start="24293" end="24306" title="impressionism" label="impressionism"/>
