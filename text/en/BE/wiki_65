<link wikidata="Q534938" start="19127" end="19142" title="Hôtel de Lauzun" label="Hôtel de Lauzun"/>
<link wikidata="Q329852" start="19447" end="19467" title="Luxembourg Palace" label="Palais du Luxembourg"/>
<link wikidata="Q329852" start="19687" end="19707" title="Luxembourg Palace" label="Palais du Luxembourg"/>
