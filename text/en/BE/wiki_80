<link wikidata="Q50415" start="2309" end="2318" title="Songpa-gu" label="Songpa-gu"/>
<link wikidata="Q8684" start="2320" end="2325" title="Seoul" label="Seoul"/>
<link wikidata="Q884" start="2327" end="2338" title="South Korea" label="South Korea"/>
<link wikidata="Q33638" start="2412" end="2418" title="Manchu language" label="Manchu"/>
<link wikidata="Q9246" start="2477" end="2486" title="Mongolian language" label="Mongolian"/>
<link wikidata="Q37041" start="2558" end="2564" title="Hanmun" label="Hanmun"/>
<link wikidata="Q484453" start="2877" end="2886" title="Injo of Joseon" label="King Injo"/>
<link wikidata="Q488547" start="3629" end="3644" title="Battle of Sarhu" label="Battle of Sarhū"/>
<link wikidata="Q491130" start="3678" end="3691" title="Gang Hong-rip" label="Gang Hong-rip"/>
<link wikidata="Q9903" start="3733" end="3745" title="Ming dynasty" label="Ming dynasty"/>
<link wikidata="Q311189" start="3858" end="3865" title="Nurhaci" label="Nurhaci"/>
<link wikidata="Q490348" start="3992" end="4020" title="Later Jin invasion of Joseon" label="Later Jin invasion of Joseon"/>
<link wikidata="Q713414" start="4713" end="4729" title="History of Korea" label="History of Korea"/>
<link wikidata="Q497159" start="4732" end="4748" title="Yeongeunmun Gate" label="Yeongeunmun Gate"/>
<link wikidata="Q497066" start="4751" end="4768" title="Independence Gate" label="Independence Gate"/>
<link wikidata="Q1779222" start="4771" end="4777" title="Kowtow" label="Kowtow"/>
<link wikidata="Q6006438" start="4780" end="4805" title="Tributary system of China" label="Tributary system of China"/>
