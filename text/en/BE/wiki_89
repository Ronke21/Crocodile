<link wikidata="Q187466" start="2833" end="2842" title="Processor register" label="registers"/>
<link wikidata="Q609843" start="3021" end="3031" title="System/360" label="System/360"/>
<link wikidata="Q322202" start="3967" end="3985" title="garbage collection (computer science)" label="garbage collection"/>
<link wikidata="Q118155" start="4055" end="4062" title="pointer (computer programming)" label="pointer"/>
<link wikidata="Q1333515" start="4093" end="4108" title="type conversion" label="type conversion"/>
<link wikidata="Q193040" start="4908" end="4923" title="embedded system" label="embedded system"/>
<link wikidata="Q4388313" start="4995" end="5004" title="unrolled linked list" label="unrolling"/>
<link wikidata="Q218825" start="5120" end="5133" title="random access" label="random access"/>
<link wikidata="Q1127410" start="6529" end="6542" title="serialization" label="serialization"/>
<link wikidata="Q623818" start="6703" end="6721" title="binary search tree" label="binary search tree"/>
