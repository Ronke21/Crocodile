<link wikidata="Q181767" start="40" end="48" title="Stress (linguistics)" label="stressed"/>
<link wikidata="Q7491739" start="318" end="326" title="scansion" label="scansion"/>
<link wikidata="Q1651051" start="512" end="516" title="foot (poetry)" label="foot"/>
<link wikidata="Q181767" start="784" end="804" title="Stress (linguistics)" label="Stress (linguistics)"/>
</links>
