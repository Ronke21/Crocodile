<link wikidata="Q1502974" start="2514" end="2519" title="Regin" label="Regin"/>
<link wikidata="Q745315" start="2628" end="2634" title="Fáfnir" label="Fafnir"/>
<link wikidata="Q850506" start="3850" end="3858" title="Brunhild" label="Brynhild"/>
