<link wikidata="Q1651051" start="58" end="62" title="metrical foot" label="feet"/>
<link wikidata="Q1651051" start="79" end="83" title="metrical foot" label="foot"/>
<link wikidata="Q7720088" start="129" end="144" title="The Bridge of Sighs (poem)" label="Bridge of Sighs"/>
</links>
