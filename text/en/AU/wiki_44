<link wikidata="Q729531" start="9483" end="9515" title="The One with Ross&amp;#x27;s Wedding" label="The One with Ross&amp;#x27;s Wedding"/>
<link wikidata="Q643263" start="9582" end="9603" title="Sense and Sensibility (film)" label="Sense and Sensibility"/>
<link wikidata="Q168724" start="9642" end="9655" title="Emma Thompson" label="Emma Thompson"/>
