<link wikidata="Q21941952" start="7710" end="7719" title="the Crown" label="the Crown"/>
<link wikidata="Q97903913" start="7754" end="7764" title="Lord Peter Wimsey (radio series)" label="adaptation"/>
<link wikidata="Q795598" start="7778" end="7789" title="BBC Radio 4" label="BBC Radio 4"/>
<link wikidata="Q350165" start="7803" end="7814" title="Simon Brett" label="Simon Brett"/>
