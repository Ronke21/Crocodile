<link wikidata="Q2621452" start="102" end="120" title="Charles F. Hockett" label="Charles F. Hockett"/>
<link wikidata="Q43249" start="179" end="187" title="morpheme" label="morpheme"/>
<link wikidata="Q43249" start="241" end="249" title="morpheme" label="morpheme"/>
<link wikidata="Q318917" start="891" end="908" title="fusional language" label="fusional language"/>
<link wikidata="Q185816" start="1582" end="1592" title="analogy" label="analogical"/>
<link wikidata="Q34698" start="1933" end="1943" title="adjective" label="adjectival"/>
