<link wikidata="Q752259" start="5443" end="5468" title="Bulgarian Socialist Party" label="Bulgarian Socialist Party"/>
<link wikidata="Q37922" start="5698" end="5723" title="Nobel Prize in Literature" label="Nobel Prize in Literature"/>
<link wikidata="Q7280506" start="5737" end="5751" title="Radichkov Peak" label="Radichkov Peak"/>
<link wikidata="Q856906" start="5755" end="5772" title="Livingston Island" label="Livingston Island"/>
<link wikidata="Q140948" start="5780" end="5802" title="South Shetland Islands" label="South Shetland Islands"/>
