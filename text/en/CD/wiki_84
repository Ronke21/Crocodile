<link wikidata="Q5145018" start="1118" end="1123" title="Jiggs, Nevada" label="Jiggs"/>
<link wikidata="Q5642689" start="1508" end="1532" title="Hall of Great Westerners" label="Hall of Great Westerners"/>
<link wikidata="Q6971981" start="1540" end="1585" title="National Cowboy &amp;amp; Western Heritage Museum" label="National Cowboy &amp;amp; Western Heritage Museum"/>
