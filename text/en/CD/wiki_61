<link wikidata="Q1353919" start="150" end="170" title="Trap (printing)" label="&amp;#x27;trapping&amp;#x27;"/>
<link wikidata="Q1409234" start="215" end="225" title="rich black" label="rich black"/>
<link wikidata="Q282942" start="773" end="789" title="returns to scale" label="returns to scale"/>
<link wikidata="Q2166429" start="1052" end="1068" title="Thermal printing" label="thermal printers"/>
</links>
