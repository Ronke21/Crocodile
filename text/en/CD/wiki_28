<link wikidata="Q1297" start="76" end="83" title="Chicago" label="Chicago"/>
<link wikidata="Q1204" start="85" end="93" title="Illinois" label="Illinois"/>
<link wikidata="Q82650" start="161" end="168" title="taxicab" label="taxicab"/>
