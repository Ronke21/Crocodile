<link wikidata="Q189863" start="252" end="263" title="Herzegovina" label="Herzegovina"/>
<link wikidata="Q272110" start="310" end="337" title="Yugoslav People&amp;#x27;s Army" label="Yugoslav People&amp;#x27;s Army"/>
<link wikidata="Q641691" start="429" end="444" title="Communist Party of Yugoslavia" label="Communist Party"/>
