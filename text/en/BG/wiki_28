<link wikidata="Q57200" start="18016" end="18039" title="St. Pete Beach, Florida" label="St. Pete Beach, Florida"/>
<link wikidata="Q1387" start="18044" end="18056" title="Rhode Island" label="Rhode Island"/>
<link wikidata="Q210826" start="18155" end="18168" title="Thoroughbreds" label="Thoroughbreds"/>
