<link wikidata="Q3304803" start="3167" end="3172" title="Mekeo language" label="Mekeo"/>
<link wikidata="Q1118286" start="3302" end="3309" title="Kanaka (Pacific Island worker)" label="Kanakas"/>
<link wikidata="Q33516" start="3410" end="3414" title="Motu language" label="Motu"/>
