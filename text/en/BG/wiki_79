<link wikidata="None" start="711" end="733" title="Schlesinger Foundation" label="Schlesinger Foundation"/>
<link wikidata="Q85355" start="979" end="988" title="Mondadori" label="Mondadori"/>
<link wikidata="Q3847219" start="1205" end="1216" title="Maria Corti" label="Maria Corti"/>
<link wikidata="Q219317" start="1250" end="1269" title="University of Pavia" label="University of Pavia"/>
<link wikidata="None" start="1567" end="1579" title="Dante Isella" label="Dante Isella"/>
