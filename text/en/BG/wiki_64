<link wikidata="Q178827" start="61" end="68" title="monomer" label="monomer"/>
<link wikidata="Q557573" start="129" end="144" title="reaction vessel" label="reaction vessel"/>
<link wikidata="Q81163" start="341" end="350" title="polymer" label="copolymer"/>
</links>
