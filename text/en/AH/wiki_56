<link wikidata="Q16206398" start="1575" end="1586" title="Heathoberht" label="Heathoberht"/>
<link wikidata="Q256506" start="1607" end="1618" title="Rabia Basri" label="Rabia Basri"/>
<link wikidata="Q9603" start="1627" end="1631" title="Sufism" label="Sufi"/>
<link wikidata="Q23532" start="1653" end="1656" title="717" label="717"/>
</links>
