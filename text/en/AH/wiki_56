<link wikidata="Q22890" start="1904" end="1911" title="Ireland" label="Ireland"/>
<link wikidata="Q334847" start="1915" end="1924" title="Ibn Ishaq" label="Ibn Ishaq"/>
<link wikidata="Q201788" start="1931" end="1940" title="historian" label="historian"/>
<link wikidata="Q208628" start="1945" end="1957" title="Hagiography" label="hagiographer"/>
<link wikidata="Q30561" start="1962" end="1965" title="AD 767" label="767"/>
<link wikidata="Q3612968" start="1969" end="1980" title="Khurshid of Tabaristan" label="Khurshid II"/>
<link wikidata="Q2064908" start="1995" end="2002" title="Spahbed" label="spāhbed"/>
<link wikidata="Q709175" start="2013" end="2023" title="Tabaristan" label="Tabaristan"/>
<link wikidata="Q23573" start="2028" end="2031" title="734" label="734"/>
<link wikidata="Q292929" start="2036" end="2044" title="Óengus I" label="Óengus I"/>
<link wikidata="Q102891" start="2058" end="2063" title="Picts" label="Picts"/>
<link wikidata="Q1328480" start="2066" end="2076" title="Shi Siming" label="Shi Siming"/>
<link wikidata="Q23500" start="2110" end="2113" title="703" label="703"/>
