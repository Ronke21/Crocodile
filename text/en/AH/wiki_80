<link wikidata="Q43806" start="3118" end="3130" title="invertebrate" label="invertebrate"/>
<link wikidata="Q16546828" start="3143" end="3148" title="gecko" label="gecko"/>
<link wikidata="Q28627" start="3788" end="3797" title="gestation" label="gestation"/>
<link wikidata="Q552431" start="4037" end="4056" title="Habitat destruction" label="Habitat destruction"/>
<link wikidata="Q5322847" start="4421" end="4468" title="EDGE Species" label="Evolutionarily Distinct and Globally Endangered"/>
<link wikidata="Q1630245" start="4554" end="4567" title="Horton Plains" label="Horton Plains"/>
<link wikidata="Q61090157" start="4870" end="4882" title="Anna Nekaris" label="Anna Nekaris"/>
<link wikidata="Q1630245" start="4886" end="4913" title="Horton Plains National Park" label="Horton Plains National Park"/>
<link wikidata="Q220198" start="4965" end="4993" title="Zoological Society of London" label="Zoological Society of London"/>
