<link wikidata="Q18986" start="5352" end="5360" title="Pergamon" label="Pergamon"/>
<link wikidata="Q26847" start="5372" end="5379" title="Galatia" label="Galatia"/>
<link wikidata="Q45881" start="5384" end="5390" title="188 BC" label="188 BC"/>
<link wikidata="Q7217" start="5407" end="5422" title="Emperor Qianshao of Han" label="Prince Liu Gong"/>
<link wikidata="Q7209" start="5491" end="5502" title="Han dynasty" label="Han dynasty"/>
