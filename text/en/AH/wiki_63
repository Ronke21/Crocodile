<link wikidata="Q16863947" start="4760" end="4781" title="Wahsudan ibn Muhammad" label="Wahsudan ibn Muhammad"/>
<link wikidata="Q28005133" start="4799" end="4805" title="Yan Xu" label="Yan Xu"/>
<link wikidata="Q5070804" start="4815" end="4825" title="Chancellor of the Tang dynasty" label="chancellor"/>
