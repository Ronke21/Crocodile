<link wikidata="Q2912" start="36016" end="36027" title="December 31" label="December 31"/>
<link wikidata="Q541596" start="36030" end="36042" title="John Moisant" label="John Moisant"/>
<link wikidata="Q7717" start="36065" end="36069" title="1868" label="1868"/>
<link wikidata="Q53010920" start="36095" end="36114" title="Emma Bedelia Dunham" label="Emma Bedelia Dunham"/>
<link wikidata="Q7004" start="36146" end="36150" title="1826" label="1826"/>
<link wikidata="Q44585" start="36175" end="36184" title="Nobel Prize in Chemistry" label="Chemistry"/>
<link wikidata="Q57127" start="36187" end="36199" title="Otto Wallach" label="Otto Wallach"/>
<link wikidata="Q37922" start="36202" end="36212" title="Nobel Prize in Literature" label="Literature"/>
<link wikidata="Q76487" start="36215" end="36225" title="Paul Heyse" label="Paul Heyse"/>
<link wikidata="Q80061" start="36228" end="36236" title="Nobel Prize in Physiology or Medicine" label="Medicine"/>
<link wikidata="Q57128" start="36239" end="36254" title="Albrecht Kossel" label="Albrecht Kossel"/>
