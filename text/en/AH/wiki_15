<link wikidata="Q6117" start="3813" end="3817" title="1403" label="1403"/>
<link wikidata="Q9965" start="3823" end="3837" title="Yongle Emperor" label="Yongle Emperor"/>
<link wikidata="Q16666" start="3870" end="3877" title="Nanjing" label="Nanjing"/>
<link wikidata="Q956" start="3881" end="3888" title="Beijing" label="Beijing"/>
<link wikidata="Q6121" start="3892" end="3896" title="1404" label="1404"/>
<link wikidata="Q6129" start="3897" end="3901" title="1406" label="1406"/>
<link wikidata="Q12504843" start="3903" end="3913" title="Regreg War" label="Regreg War"/>
<link wikidata="Q49326" start="3915" end="3924" title="Majapahit" label="Majapahit"/>
<link wikidata="Q12452603" start="3956" end="3971" title="Wikramawardhana" label="Wikramawardhana"/>
<link wikidata="Q6124" start="3993" end="3997" title="1405" label="1405"/>
<link wikidata="Q422065" start="4003" end="4020" title="Sultanate of Sulu" label="Sultanate of Sulu"/>
<link wikidata="Q7489673" start="4039" end="4055" title="Sharif ul-Hāshim of Sulu" label="Sharif ul-Hāshim"/>
<link wikidata="Q6124" start="4059" end="4063" title="1405" label="1405"/>
