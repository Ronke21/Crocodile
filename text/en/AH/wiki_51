<link wikidata="Q307" start="429" end="444" title="Galileo Galilei" label="Galileo Galilei"/>
<link wikidata="Q8963" start="449" end="464" title="Johannes Kepler" label="Johannes Kepler"/>
<link wikidata="Q598774" start="526" end="544" title="Polish–Swedish War (1600–1611)" label="Polish-Swedish War"/>
<link wikidata="Q717073" start="553" end="574" title="Battle of Kokenhausen" label="Battle of Kokenhausen"/>
