<link wikidata="Q35900" start="136" end="148" title="Omar Khayyam" label="Omar Khayyam"/>
<link wikidata="Q37594" start="151" end="172" title="William the Conqueror" label="William the Conqueror"/>
<link wikidata="Q293626" start="175" end="188" title="Al-Qa&amp;#x27;im (Abbasid caliph at Baghdad)" label="Al-Qa&amp;#x27;im"/>
<link wikidata="Q200193" start="191" end="201" title="Alp Arslan" label="Alp Arslan"/>
<link wikidata="Q311482" start="218" end="230" title="Malik-Shah I" label="Malik-Shah I"/>
</links>
