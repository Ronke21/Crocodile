<link wikidata="Q9026959" start="139" end="148" title="holograph" label="holograph"/>
<link wikidata="Q692" start="558" end="569" title="William Shakespeare" label="Shakespeare"/>
<link wikidata="Q104871" start="583" end="611" title="Midsummer Night&amp;#x27;s Dream" label="Midsummer Night&amp;#x27;s Dream"/>
