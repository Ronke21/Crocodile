<link wikidata="None" start="133" end="145" title="Abram Petkau" label="Abram Petkau"/>
<link wikidata="Q757601" start="153" end="176" title="Atomic Energy of Canada" label="Atomic Energy of Canada"/>
<link wikidata="Q6119842" start="177" end="218" title="Whiteshell Nuclear Research Establishment" label="Whiteshell Nuclear Research Establishment"/>
<link wikidata="Q1948" start="220" end="228" title="Manitoba" label="Manitoba"/>
<link wikidata="Q29548" start="586" end="599" title="cell membrane" label="cell membrane"/>
