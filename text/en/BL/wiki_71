<link wikidata="Q59157" start="27" end="49" title="educational psychology" label="educational psychology"/>
<link wikidata="Q5156299" start="51" end="72" title="Competency evaluation (language)" label="competency evaluation"/>
<link wikidata="Q3244907" start="166" end="183" title="standardized test" label="standardized test"/>
