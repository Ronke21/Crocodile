<link wikidata="Q356097" start="3322" end="3334" title="Operation Magic Carpet" label="Magic Carpet"/>
<link wikidata="Q2634" start="3380" end="3386" title="Naples" label="Naples"/>
<link wikidata="Q2946066" start="3468" end="3490" title="Atlantic Reserve Fleet" label="Atlantic Reserve Fleet"/>
<link wikidata="Q8663" start="3565" end="3575" title="Korean War" label="Korean War"/>
<link wikidata="Q486306" start="3696" end="3714" title="Pensacola, Florida" label="Pensacola, Florida"/>
