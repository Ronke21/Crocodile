<link wikidata="Q783" start="4092" end="4100" title="Honduras" label="Honduras"/>
<link wikidata="Q1345" start="4283" end="4295" title="Philadelphia" label="Philadelphia"/>
<link wikidata="Q2035234" start="4387" end="4398" title="battle star" label="battle star"/>
