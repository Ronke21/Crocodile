<link wikidata="Q61197" start="231" end="259" title="Johann Christian Poggendorff" label="Johann Christian Poggendorff"/>
<link wikidata="Q61357" start="321" end="350" title="Johann Karl Friedrich Zöllner" label="Johann Karl Friedrich Zöllner"/>
<link wikidata="Q655333" start="410" end="426" title="Zöllner illusion" label="Zöllner illusion"/>
