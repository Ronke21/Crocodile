<link wikidata="Q207461" start="86" end="105" title="Geography of Spain" label="territorial borders"/>
<link wikidata="Q910648" start="259" end="278" title="Spanish East Indies" label="Spanish East Indies"/>
<link wikidata="Q12583" start="304" end="324" title="Spanish–American War" label="Spanish–American War"/>
<link wikidata="Q664603" start="363" end="384" title="German–Spanish Treaty (1899)" label="German–Spanish Treaty"/>
<link wikidata="Q1024362" start="490" end="523" title="Spanish National Research Council" label="Spanish National Research Council"/>
