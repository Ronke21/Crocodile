<link wikidata="Q1563199" start="66" end="70" title="Chinese sovereign" label="king"/>
<link wikidata="Q35216" start="87" end="99" title="Zhou dynasty" label="Zhou dynasty"/>
<link wikidata="Q1202938" start="126" end="135" title="King Huan of Zhou" label="King Huan"/>
<link wikidata="Q261333" start="175" end="182" title="King Xi of Zhou" label="King Xi"/>
