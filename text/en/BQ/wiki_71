<link wikidata="Q170439" start="104" end="109" title="chord (music)" label="chord"/>
<link wikidata="Q424494" start="431" end="437" title="Accent (music)" label="accent"/>
<link wikidata="Q4043755" start="1065" end="1079" title="Martelé (bowstroke)" label="martelé bowing"/>
