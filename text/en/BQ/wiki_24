<link wikidata="Q5684863" start="5442" end="5470" title="Riddle State Nature Preserve" label="Riddle State Nature Preserve"/>
<link wikidata="Q4813476" start="5485" end="5503" title="Athens Conservancy" label="Athens Conservancy"/>
