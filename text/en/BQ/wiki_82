<link wikidata="Q953421" start="7349" end="7368" title="Panteion University" label="Panteion University"/>
<link wikidata="" start="7386" end="7405" title=":gr:Ιωάννης Πρίγκος" label=":gr:Ιωάννης Πρίγκος"/>
<link wikidata="Q7840171" start="7418" end="7438" title="Saint Triantaphyllos" label="Saint Triantaphyllos"/>
