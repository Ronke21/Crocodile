<link wikidata="Q1867613" start="178" end="210" title="Lockheed Model 12 Electra Junior" label="Lockheed Model 12 Electra Junior"/>
<link wikidata="Q1808230" start="275" end="306" title="Lockheed Model 14 Super Electra" label="Lockheed Model 14 Super Electra"/>
