<link wikidata="Q16985948" start="13971" end="14002" title="Independent telephone companies" label="Independent telephone companies"/>
<link wikidata="Q7624369" start="14057" end="14074" title="Stromberg-Carlson" label="Stromberg-Carlson"/>
