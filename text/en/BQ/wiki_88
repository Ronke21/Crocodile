<link wikidata="Q265193" start="53" end="72" title="Microsatellite" label="short tandem repeat"/>
<link wikidata="Q485680" start="86" end="98" title="Polymorphism (biology)" label="polymorphism"/>
<link wikidata="Q767511" start="104" end="118" title="genetic marker" label="genetic marker"/>
<link wikidata="Q863908" start="151" end="163" title="DNA sequence" label="DNA sequence"/>
<link wikidata="Q8333" start="170" end="173" title="FBI" label="FBI"/>
<link wikidata="Q106227" start="199" end="203" title="Locus (genetics)" label="loci"/>
<link wikidata="Q912147" start="555" end="565" title="genotyping" label="genotyping"/>
