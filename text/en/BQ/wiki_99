<link wikidata="Q13376" start="105" end="112" title="Pistoia" label="Pistoia"/>
<link wikidata="Q7357071" start="432" end="447" title="Rodney Slatford" label="Rodney Slatford"/>
<link wikidata="Q1422837" start="488" end="506" title="Grove Dictionary of Music and Musicians" label="Grove Music Online"/>
