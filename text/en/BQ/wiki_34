<link wikidata="Q30971" start="4865" end="4881" title="French Polynesia" label="French Polynesia"/>
<link wikidata="Q252" start="4886" end="4895" title="Indonesia" label="Indonesia"/>
<link wikidata="Q131747027" start="5089" end="5099" title="Spawn (biology)" label="egg-caring"/>
