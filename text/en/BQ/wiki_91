<link wikidata="Q6674" start="24126" end="24131" title="devil" label="devil"/>
<link wikidata="Q668460" start="24328" end="24337" title="Muʿtazila" label="Muʿtazila"/>
<link wikidata="Q1821239" start="24473" end="24489" title="Day of Judgement" label="Day of Judgement"/>
<link wikidata="None" start="24511" end="24523" title="Dirar b. Amr" label="Dirar b. Amr"/>
<link wikidata="Q381240" start="24686" end="24718" title="Isra and Mi&amp;#x27;raj" label="Muhammad&amp;#x27;s heavenly journey"/>
<link wikidata="Q809688" start="24724" end="24746" title="Barzakh" label="the life in the graves"/>
<link wikidata="Q1135949" start="24774" end="24817" title="Fall of man" label="Adam and Eve&amp;#x27;s expulsion from paradise"/>
<link wikidata="Q5760618" start="24917" end="24934" title="Hikmah" label="God&amp;#x27;s wisdom"/>
<link wikidata="Q558205" start="25155" end="25177" title="Khwaja Abdullah Ansari" label="Khwaja Abdullah Ansari"/>
