* &quot;Alejandra/Alexandra&quot; = &quot;Sandra, Ale, Álex, Alexa, Lexa, Aleja, Jandra, Chandra, Jana, Lala&quot;
* &quot;Alejandro&quot; = &quot;Ale, Álex, Alejo, Jandro, Jano, Cano, Sandro, Pando&quot;
* &quot;Alfonso&quot; = &quot;Alfon, Fon, Fonso, Fonsi, Poncho, Loncho&quot;
