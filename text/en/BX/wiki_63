<link wikidata="Q2044" start="18983" end="18991" title="Florence" label="Florence"/>
<link wikidata="Q723329" start="19234" end="19250" title="Ascot, Berkshire" label="Ascot, Berkshire"/>
<link wikidata="Q145" start="19259" end="19273" title="United Kingdom" label="United Kingdom"/>
