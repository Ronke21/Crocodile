<link wikidata="Q8442" start="6471" end="6488" title="Otto von Bismarck" label="Otto von Bismarck"/>
<link wikidata="Q214524" start="6531" end="6549" title="Gettysburg Address" label="Gettysburg Address"/>
<link wikidata="Q91" start="6553" end="6568" title="Abraham Lincoln" label="Abraham Lincoln"/>
<link wikidata="Q4668993" start="6694" end="6725" title="Lincoln&amp;#x27;s second inaugural address" label="Lincoln&amp;#x27;s Second Inaugural"/>
<link wikidata="None" start="6816" end="6885" title="Susan B. Anthony#United States v. Susan B. Anthony" label="&amp;quot;Is it a Crime for a Citizen of the United States to Vote?&amp;quot;"/>
<link wikidata="Q192245" start="6896" end="6912" title="Susan B. Anthony" label="Susan B. Anthony"/>
<link wikidata="Q3246745" start="6945" end="6989" title="History of women&amp;#x27;s suffrage in the United States" label="women&amp;#x27;s suffrage into the United States"/>
<link wikidata="Q5162861" start="7035" end="7058" title="consent of the governed" label="consent of the governed"/>
<link wikidata="None" start="7118" end="7153" title="Chief Joseph#Nez Perce War" label="Surrender of Nez Perce Chief Joseph"/>
<link wikidata="Q2119069" start="7209" end="7242" title="Dostoyevsky&amp;#x27;s Pushkin Speech" label="Dostoyevsky&amp;#x27;s Pushkin Speech"/>
<link wikidata="Q991" start="7266" end="7284" title="Fyodor Dostoyevsky" label="Fyodor Dostoyevsky"/>
<link wikidata="Q7200" start="7315" end="7332" title="Alexander Pushkin" label="Alexander Pushkin"/>
<link wikidata="None" start="7347" end="7364" title="Russell Conwell#&amp;quot;Acres of Diamonds&amp;quot;" label="Acres of Diamonds"/>
