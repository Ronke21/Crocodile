<link wikidata="Q7206531" start="14074" end="14094" title="Pocono Mountains 125" label="Pocono Mountains 125"/>
<link wikidata="Q217238" start="14275" end="14289" title="Jimmie Johnson" label="Jimmie Johnson"/>
<link wikidata="Q201820" start="14298" end="14308" title="Kurt Busch" label="Kurt Busch"/>
