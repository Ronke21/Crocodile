<link wikidata="Q3425168" start="446" end="499" title="National Institute for Health and Clinical Excellence" label="National Institute for Health and Clinical Excellence"/>
<link wikidata="Q4823468" start="540" end="595" title="Australasian Society of Clinical Immunology and Allergy" label="Australasian Society of Clinical Immunology and Allergy"/>
<link wikidata="Q4742865" start="601" end="651" title="American Academy of Allergy, Asthma and Immunology" label="American Academy of Allergy, Asthma and Immunology"/>
<link wikidata="Q5146807" start="899" end="944" title="College of Physicians and Surgeons of Ontario" label="College of Physicians and Surgeons of Ontario"/>
