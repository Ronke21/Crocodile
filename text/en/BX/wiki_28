<link wikidata="Q21199" start="43" end="57" title="natural number" label="natural number"/>
<link wikidata="Q717063" start="68" end="71" title="124 (number)" label="124"/>
<link wikidata="Q223661" start="86" end="89" title="126 (number)" label="126"/>
<link wikidata="Q861555" start="122" end="126" title="cubic number" label="cube"/>
<link wikidata="Q203" start="130" end="131" title="5 (number)" label="5"/>
