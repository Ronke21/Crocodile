<link wikidata="Q46" start="332" end="338" title="Europe" label="Europe"/>
<link wikidata="Q49" start="345" end="358" title="North America" label="North America"/>
<link wikidata="Q786" start="1301" end="1319" title="Dominican Republic" label="Dominican Republic"/>
