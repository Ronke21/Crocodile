<link wikidata="Q131877" start="3214" end="3220" title="rubber" label="rubber"/>
<link wikidata="Q17148556" start="3360" end="3392" title="Double-lumen endobronchial tube" label="double-lumen endobronchial tubes"/>
<link wikidata="Q2964004" start="3397" end="3413" title="thoracic surgery" label="thoracic surgery"/>
<link wikidata="None" start="3427" end="3450" title="single-lung ventilation" label="single-lung ventilation"/>
<link wikidata="Q16606552" start="5290" end="5313" title="obstructive sleep apnea" label="obstructive sleep apnea"/>
<link wikidata="Q1727193" start="5581" end="5595" title="Laryngeal tube" label="Laryngeal tube"/>
<link wikidata="Q750195" start="5598" end="5617" title="Tracheal intubation" label="Tracheal intubation"/>
