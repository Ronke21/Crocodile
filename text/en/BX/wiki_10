<link wikidata="Q5497824" start="5457" end="5495" title="Frederick Galleghan" label="Sir F &amp;#x27;Black Jack&amp;#x27; Galleghan"/>
<link wikidata="Q5153828" start="5572" end="5606" title="Commonwealth Investigation Service" label="Commonwealth Investigation Service"/>
<link wikidata="Q390216" start="6209" end="6230" title="Sydney Morning Herald" label="Sydney Morning Herald"/>
