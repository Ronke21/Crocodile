<link wikidata="Q41323" start="87" end="104" title="American football" label="American football"/>
<link wikidata="Q5305993" start="259" end="268" title="draw play" label="draw play"/>
<link wikidata="Q528145" start="604" end="614" title="linebacker" label="linebacker"/>
