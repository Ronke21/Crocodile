<link wikidata="Q6094484" start="4" end="25" title="Ithaca Community News" label="Ithaca Community News"/>
<link wikidata="Q7150920" start="121" end="132" title="Paul Glover (activist)" label="Paul Glover"/>
<link wikidata="Q217346" start="253" end="263" title="Ithaca, NY" label="Ithaca, NY"/>
