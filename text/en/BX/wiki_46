<link wikidata="Q1195339" start="57" end="79" title="directed acyclic graph" label="directed acyclic graph"/>
<link wikidata="Q1415372" start="100" end="104" title="Path (graph theory)" label="path"/>
<link wikidata="Q177929" start="118" end="123" title="Stack (data structure)" label="stack"/>
<link wikidata="Q1760114" start="176" end="199" title="GLR parser" label="Tomita&amp;#x27;s algorithm"/>
<link wikidata="Q177929" start="229" end="234" title="Stack (data structure)" label="stack"/>
<link wikidata="Q751443" start="240" end="258" title="pushdown automaton" label="pushdown automaton"/>
