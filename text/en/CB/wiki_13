<link wikidata="Q3067003" start="107" end="133" title="Farrar Straus &amp;amp; Giroux" label="Farrar Straus &amp;amp; Giroux"/>
<link wikidata="Q60195" start="335" end="343" title="semiotic" label="semiotic"/>
<link wikidata="Q147027" start="813" end="831" title="thought experiment" label="thought experiment"/>
<link wikidata="Q60195" start="1585" end="1597" title="semiotics" label="semioticians"/>
<link wikidata="Q363" start="1651" end="1664" title="World War III" label="World War III"/>
