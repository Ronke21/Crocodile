<link wikidata="Q192234" start="30" end="41" title="camera lens" label="camera lens"/>
<link wikidata="Q6195073" start="78" end="89" title="Jim Frazier (inventor)" label="Jim Frazier"/>
<link wikidata="Q215932" start="144" end="158" title="depth of field" label="depth of field"/>
