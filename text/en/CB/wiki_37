<link wikidata="Q29468" start="95" end="111" title="United States Republican Party" label="Republican Party"/>
<link wikidata="Q11701" start="138" end="167" title="United States House of Representatives" label="U.S. House of Representatives"/>
<link wikidata="Q685335" start="239" end="255" title="Portsmouth, Ohio" label="Portsmouth, Ohio"/>
