<link wikidata="Q2610597" start="129" end="148" title="Euthydemus of Chios" label="Euthydemus of Chios"/>
<link wikidata="Q123397" start="288" end="296" title="Republic (Plato)" label="Republic"/>
<link wikidata="Q1373343" start="369" end="380" title="Memorabilia (Xenophon)" label="Memorabilia"/>
<link wikidata="Q362604" start="388" end="400" title="Euthydemus I" label="Euthydemus I"/>
