<link wikidata="Q326024" start="16755" end="16779" title="Simone Antonio Saint-Bon" label="Simone Antonio Saint-Bon"/>
<link wikidata="Q8680" start="16811" end="16818" title="British Empire" label="British"/>
<link wikidata="Q517" start="16942" end="16950" title="Napoleon" label="Napoleon"/>
