<link wikidata="Q7268946" start="14" end="33" title="Quantity adjustment" label="quantity adjustment"/>
<link wikidata="Q166656" start="211" end="228" title="supply and demand" label="supply and demand"/>
<link wikidata="Q836133" start="229" end="243" title="economic equilibrium" label="disequilibrium"/>
<link wikidata="Q37654" start="249" end="255" title="Market (economics)" label="market"/>
<link wikidata="Q2516951" start="297" end="304" title="pricing" label="pricing"/>
