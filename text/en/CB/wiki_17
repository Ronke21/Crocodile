<link wikidata="Q43015" start="118" end="125" title="finance" label="finance"/>
<link wikidata="Q243460" start="155" end="171" title="income statement" label="income statement"/>
<link wikidata="Q191891" start="2010" end="2019" title="Fiscal year" label="quarterly"/>
<link wikidata="Q850210" start="2020" end="2027" title="revenue" label="revenue"/>
