<link wikidata="Q4789178" start="39" end="54" title="Argadnel" label="Maig Arggatnéul"/>
<link wikidata="Q598810" start="166" end="182" title="Bran the Blessed" label="Bran the Blessed"/>
<link wikidata="Q6867" start="198" end="230" title="International Astronomical Union" label="International Astronomical Union"/>
<link wikidata="Q751439" start="254" end="259" title="Regio (astronomy)" label="regio"/>
