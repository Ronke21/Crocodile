<link wikidata="None" start="340" end="365" title="Herbert Eugene Westervelt" label="Herbert Eugene Westervelt"/>
<link wikidata="Q6119445" start="431" end="447" title="Jacob Westervelt (sheriff)" label="Jacob Westervelt"/>
