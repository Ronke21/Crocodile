<link wikidata="Q41252" start="123" end="131" title="Bromberg" label="Bromberg"/>
<link wikidata="Q41252" start="137" end="146" title="Bydgoszcz" label="Bydgoszcz"/>
<link wikidata="Q214203" start="183" end="186" title="BWV" label="BWV"/>
<link wikidata="Q1339" start="280" end="301" title="Johann Sebastian Bach" label="Johann Sebastian Bach"/>
<link wikidata="Q2293299" start="582" end="610" title="Italian Concerto (Bach)" label="&amp;quot;Italian Concerto&amp;quot;"/>
<link wikidata="Q50662" start="702" end="729" title="Goethe University Frankfurt" label="Goethe University Frankfurt"/>
