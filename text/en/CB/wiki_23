<link wikidata="Q362" start="131" end="143" title="World War II" label="World War II"/>
<link wikidata="Q650203" start="190" end="208" title="Deutsches Jungvolk" label="Deutsches Jungvolk"/>
<link wikidata="Q38791" start="225" end="237" title="Hitler Youth" label="Hitler Youth"/>
<link wikidata="Q44294" start="336" end="354" title="Ford Motor Company" label="Ford Motor Company"/>
