<link wikidata="Q255147" start="1906" end="1927" title="Yale University Press" label="Yale University Press"/>
<link wikidata="Q16949679" start="2019" end="2031" title="Gotham Books" label="Gotham Books"/>
<link wikidata="Q4972414" start="2101" end="2115" title="Broadway Books" label="Broadway Books"/>
