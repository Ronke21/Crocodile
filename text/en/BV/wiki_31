<link wikidata="Q143739" start="239" end="246" title="beeswax" label="beeswax"/>
<link wikidata="Q839494" start="254" end="266" title="carnauba wax" label="carnauba wax"/>
<link wikidata="Q177463" start="302" end="310" title="charcoal" label="charcoal"/>
<link wikidata="Q816713" start="312" end="319" title="sawdust" label="sawdust"/>
<link wikidata="Q81944" start="395" end="402" title="cutlery" label="cutlers"/>
