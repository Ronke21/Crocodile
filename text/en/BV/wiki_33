<link wikidata="Q1177366" start="4951" end="4967" title="David de Kretser" label="David de Kretser"/>
<link wikidata="Q7753244" start="5011" end="5044" title="The Natural Confectionery Company" label="The Natural Confectionery Company"/>
<link wikidata="Q731912" start="5460" end="5467" title="Carrier Corporation" label="Carrier"/>
