<link wikidata="Q162720" start="46" end="59" title="Riario" label="Riario-Sforza"/>
<link wikidata="Q13320" start="93" end="99" title="Savona" label="Savona"/>
<link wikidata="Q1449" start="106" end="111" title="Genoa" label="Genoa"/>
<link wikidata="Q170174" start="141" end="153" title="Papal States" label="Papal States"/>
