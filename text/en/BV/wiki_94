<link wikidata="Q7246235" start="29" end="32" title="Private product remaining" label="PPR"/>
<link wikidata="Q6803767" start="115" end="118" title="GNP" label="GNP"/>
<link wikidata="Q1629575" start="274" end="293" title="calculation problem" label="calculation problem"/>
<link wikidata="Q297079" start="322" end="337" title="Murray Rothbard" label="Murray Rothbard"/>
<link wikidata="Q7246235" start="352" end="355" title="Private product remaining" label="GPP"/>
<link wikidata="Q7246235" start="384" end="387" title="Private product remaining" label="PPR"/>
<link wikidata="Q12638" start="895" end="917" title="Gross Domestic Product" label="Gross Domestic Product"/>
</links>
