<link wikidata="Q924" start="3610" end="3618" title="Tanzania" label="Tanzania"/>
<link wikidata="Q9017179" start="3621" end="3626" title="kanzu" label="kanzu"/>
<link wikidata="Q1778377" start="3631" end="3636" title="Kofia (hat)" label="kofia"/>
