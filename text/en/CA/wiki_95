<link wikidata="Q81571" start="48" end="54" title="Pascal (programming language)" label="Pascal"/>
<link wikidata="Q1439853" start="182" end="212" title="Chess Engines Grand Tournament" label="Chess Engines Grand Tournament"/>
<link wikidata="Q1138485" start="372" end="378" title="Crafty" label="Crafty"/>
<link wikidata="Q1122588" start="630" end="644" title="Computer chess" label="Computer chess"/>
