<link wikidata="Q192969" start="55" end="67" title="Computer multitasking" label="multitasking"/>
<link wikidata="Q16969336" start="120" end="144" title="cooperative multitasking" label="cooperative multitasking"/>
<link wikidata="Q483318" start="180" end="187" title="Symbian" label="Symbian"/>
<link wikidata="Q216601" start="236" end="243" title="Object (computer science)" label="objects"/>
<link wikidata="Q752741" start="265" end="277" title="Asynchronous I/O" label="asynchronous"/>
