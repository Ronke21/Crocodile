<link wikidata="None" start="5113" end="5129" title="Parochial school#United States" label="parochial school"/>
<link wikidata="Q1364360" start="5167" end="5192" title="Roman Catholic Archdiocese of Cincinnati" label="Archdiocese of Cincinnati"/>
