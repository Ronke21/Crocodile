<link wikidata="Q677023" start="55" end="84" title="Multiprotocol Label Switching" label="Multiprotocol Label Switching"/>
<link wikidata="Q220501" start="415" end="433" title="Quality of service" label="Quality of service"/>
<link wikidata="Q592462" start="468" end="493" title="Forwarding equivalence class" label="forward equivalence class"/>
