<link wikidata="Q1679954" start="0" end="12" title="James Angell" label="James Angell"/>
<link wikidata="Q6130543" start="30" end="50" title="James Burrill Angell" label="James Burrill Angell"/>
<link wikidata="Q663485" start="171" end="191" title="James Rowland Angell" label="James Rowland Angell"/>
