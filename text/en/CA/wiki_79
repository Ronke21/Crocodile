<link wikidata="Q60179" start="46" end="58" title="Viewing frustum" label="view frustum"/>
<link wikidata="Q5447114" start="144" end="157" title="field of view in video games" label="field of view"/>
<link wikidata="Q1900281" start="163" end="174" title="Perspective (graphical)" label="perspective"/>
<link wikidata="Q3561045" start="175" end="196" title="virtual camera system" label="virtual camera system"/>
<link wikidata="Q846235" start="262" end="269" title="frustum" label="frustum"/>
