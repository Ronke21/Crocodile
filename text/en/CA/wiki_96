<link wikidata="Q133477" start="257" end="267" title="Ammi majus" label="Ammi majus"/>
<link wikidata="Q164091" start="351" end="369" title="Houttuynia cordata" label="Houttuynia cordata"/>
<link wikidata="Q413254" start="431" end="449" title="Ajwain" label="Trachyspermum ammi"/>
