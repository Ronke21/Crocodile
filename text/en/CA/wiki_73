<link wikidata="Q55818" start="70" end="83" title="impact crater" label="impact crater"/>
<link wikidata="Q108418" start="87" end="98" title="Cook County, Illinois" label="Cook County"/>
<link wikidata="Q1204" start="100" end="108" title="Illinois" label="Illinois"/>
<link wikidata="Q386961" start="179" end="190" title="Des Plaines, Illinois" label="Des Plaines"/>
