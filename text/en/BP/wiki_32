<link wikidata="Q840339" start="205" end="212" title="Lazarus and Dives" label="Lazarus"/>
<link wikidata="Q18813" start="228" end="241" title="New Testament" label="New Testament"/>
<link wikidata="Q208555" start="242" end="249" title="parable" label="parable"/>
