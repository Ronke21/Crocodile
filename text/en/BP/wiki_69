<link wikidata="Q1250541" start="59" end="75" title="Monnaie de Paris" label="Paris Medal Mint"/>
<link wikidata="Q517" start="111" end="131" title="Napoleon I of France" label="Napoleon I of France"/>
<link wikidata="Q7750" start="306" end="322" title="Louis XVIII of France" label="King Louis XVIII"/>
<link wikidata="Q428352" start="598" end="604" title="patina" label="patina"/>
<link wikidata="Q1918987" start="866" end="882" title="Napoléon Victor Jérôme Frédéric Bonaparte" label="Victor Bonaparte"/>
