<link wikidata="Q157692" start="31" end="53" title="Slavic names" label="Slavic male given name"/>
<link wikidata="Q840454" start="92" end="103" title="West Slavs" label="West Slavic"/>
<link wikidata="Q809" start="205" end="211" title="Polish language" label="Polish"/>
<link wikidata="Q13248" start="311" end="324" title="Upper Sorbian language" label="Upper Sorbian"/>
