<link wikidata="Q4503831" start="351" end="360" title="Objective (goal)" label="objective"/>
<link wikidata="Q7887140" start="376" end="380" title="Unit of action" label="unit"/>
<link wikidata="Q7798439" start="734" end="746" title="Through line" label="through line"/>
<link wikidata="Q17738" start="774" end="784" title="A New Hope" label="A New Hope"/>
<link wikidata="Q181803" start="835" end="853" title="Return of the Jedi" label="Return of the Jedi"/>
