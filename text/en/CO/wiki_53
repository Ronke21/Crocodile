<link wikidata="Q5123251" start="51" end="69" title="City Island Harbor" label="City Island Harbor"/>
<link wikidata="Q2646503" start="148" end="159" title="Hart Island (New York)" label="Hart Island"/>
<link wikidata="Q56063488" start="182" end="198" title="Manhattan schist" label="Manhattan schist"/>
<link wikidata="Q570309" start="199" end="206" title="bedrock" label="bedrock"/>
<link wikidata="Q7411" start="333" end="338" title="Dutch language" label="Dutch"/>
<link wikidata="Q101935" start="372" end="380" title="Huguenot" label="Huguenot"/>
