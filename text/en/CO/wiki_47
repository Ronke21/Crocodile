<link wikidata="Q1172466" start="84" end="102" title="communication link" label="communication link"/>
<link wikidata="Q2357982" start="218" end="229" title="attenuation" label="attenuation"/>
<link wikidata="Q230187" start="233" end="251" title="near-end crosstalk" label="near-end crosstalk"/>
<link wikidata="Q2357982" start="355" end="366" title="attenuation" label="attenuation"/>
