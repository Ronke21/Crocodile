<link wikidata="Q1770695" start="108" end="128" title="Hindustani classical music" label="classical Hindustani"/>
<link wikidata="Q4693687" start="141" end="153" title="Agra gharana" label="Agra gharana"/>
<link wikidata="Q7929157" start="200" end="212" title="Vijay Kichlu" label="Vijay Kichlu"/>
