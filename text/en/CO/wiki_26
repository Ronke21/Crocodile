<link wikidata="Q5164918" start="57" end="60" title="Contamination delay" label="tcd"/>
<link wikidata="Q5164918" start="459" end="462" title="Contamination delay" label="tcd"/>
<link wikidata="Q5164918" start="692" end="695" title="Contamination delay" label="tcd"/>
<link wikidata="Q183406" start="1194" end="1203" title="flip-flop (electronics)" label="flip-flop"/>
<link wikidata="Q1144416" start="1671" end="1688" title="propagation delay" label="propagation delay"/>
<link wikidata="Q616554" start="1743" end="1747" title="race condition" label="race"/>
<link wikidata="Q4044531" start="1878" end="1893" title="Morgan Kaufmann Publishers" label="Morgan Kaufmann"/>
</links>
</doc>
