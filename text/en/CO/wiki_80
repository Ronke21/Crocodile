<link wikidata="Q3004206" start="5033" end="5045" title="Wipeout 2097" label="Wipeout 2097"/>
<link wikidata="Q5305992" start="5249" end="5265" title="pop-up (video gaming)" label="graphical pop-up"/>
<link wikidata="Q281243" start="5311" end="5320" title="Eurogamer" label="Eurogamer"/>
