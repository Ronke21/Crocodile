<link wikidata="Q7735134" start="1625" end="1647" title="The Fran Drescher Show" label="The Fran Drescher Show"/>
<link wikidata="Q2741988" start="1730" end="1742" title="Love Letters (play)" label="Love Letters"/>
<link wikidata="Q464865" start="1768" end="1777" title="The Nanny" label="The Nanny"/>
