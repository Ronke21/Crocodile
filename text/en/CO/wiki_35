<link wikidata="Q5729591" start="42" end="61" title="Henry Vollam Morton" label="Henry Vollam Morton"/>
<link wikidata="Q3296078" start="103" end="110" title="Methuen Publishing" label="Methuen"/>
<link wikidata="Q45529" start="172" end="177" title="ghost" label="ghost"/>
<link wikidata="Q80837" start="187" end="199" title="supernatural" label="supernatural"/>
<link wikidata="Q1930187" start="411" end="421" title="journalist" label="journalist"/>
