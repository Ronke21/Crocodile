<link wikidata="Q697327" start="308" end="315" title="lawsuit" label="lawsuit"/>
<link wikidata="Q6302997" start="379" end="395" title="judicial opinion" label="judicial opinion"/>
<link wikidata="Q93288" start="515" end="527" title="contract law" label="contract law"/>
<link wikidata="Q1054726" start="555" end="581" title="Contract of sale" label="sale of goods and services"/>
<link wikidata="Q1640717" start="921" end="941" title="ecclesiastical court" label="ecclesiastical court"/>
<link wikidata="None" start="1028" end="1048" title="House of Convocation" label="House of Convocation"/>
