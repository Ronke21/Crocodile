<link wikidata="Q958594" start="98" end="118" title="Francis A. Schaeffer" label="Francis A. Schaeffer"/>
<link wikidata="Q7736914" start="435" end="455" title="The God Who Is There" label="The God Who Is There"/>
<link wikidata="Q7736914" start="549" end="569" title="The God Who Is There" label="The God Who Is There"/>
