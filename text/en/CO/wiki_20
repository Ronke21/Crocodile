<link wikidata="Q7272608" start="60" end="74" title="Quirpon Island" label="Quirpon Island"/>
<link wikidata="Q48335" start="76" end="88" title="Newfoundland (island)" label="Newfoundland"/>
<link wikidata="Q1450700" start="641" end="658" title="French nobility" label="French noblewoman"/>
<link wikidata="Q6760829" start="660" end="683" title="Marguerite de La Rocque" label="Marguerite de La Rocque"/>
<link wikidata="Q5664602" start="1102" end="1128" title="Harrington Harbour, Quebec" label="Harrington Harbour, Quebec"/>
<link wikidata="Q1111214" start="1184" end="1199" title="Johannes Ruysch" label="Johannes Ruysch"/>
