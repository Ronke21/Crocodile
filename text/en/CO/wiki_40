<link wikidata="Q2958827" start="737" end="743" title="Diseae" label="Diseae"/>
<link wikidata="Q143951" start="745" end="757" title="Cranichideae" label="Cranichideae"/>
<link wikidata="Q1075192" start="759" end="769" title="Chloraeinae" label="Chloraeeae"/>
