<link wikidata="Q217491" start="170" end="191" title="Network Time Protocol" label="Network Time Protocol"/>
<link wikidata="Q6781885" start="218" end="243" title="Marzullo&amp;#x27;s algorithm" label="Marzullo&amp;#x27;s algorithm"/>
<link wikidata="Q1052698" start="764" end="770" title="jitter" label="jitter"/>
