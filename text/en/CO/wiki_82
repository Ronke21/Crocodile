<link wikidata="Q12078" start="99" end="105" title="cancer" label="cancer"/>
<link wikidata="Q1365309" start="214" end="220" title="glioma" label="glioma"/>
<link wikidata="Q172847" start="305" end="316" title="polypeptide" label="polypeptide"/>
