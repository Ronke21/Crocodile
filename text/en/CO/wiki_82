<link wikidata="Q216838" start="294" end="308" title="Robert Southey" label="Robert Southey"/>
<link wikidata="Q192068" start="414" end="425" title="eclecticism" label="eclecticism"/>
<link wikidata="Q33203" start="623" end="632" title="Methodist" label="Methodist"/>
