<link wikidata="Q877670" start="11837" end="11848" title="Muzaffarids (Iran)" label="Muzaffarids"/>
<link wikidata="Q928828" start="12157" end="12162" title="Gilan" label="Gilan"/>
<link wikidata="Q638064" start="12167" end="12174" title="Shirvan" label="Shirvan"/>
<link wikidata="Q262" start="12396" end="12403" title="Algeria" label="Algeria"/>
