<link wikidata="Q1658770" start="1052" end="1077" title="Mahmud II of Great Seljuk" label="Mahmud II of Great Seljuk"/>
<link wikidata="None" start="1092" end="1102" title="Toghrül II" label="Toghrül II"/>
<link wikidata="Q293621" start="1148" end="1161" title="al-Mustarshid" label="al-Mustarshid"/>
