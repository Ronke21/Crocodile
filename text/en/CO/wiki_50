<link wikidata="Q49108" start="4237" end="4240" title="Massachusetts Institute of Technology" label="MIT"/>
<link wikidata="Q331163" start="4417" end="4439" title="Kanellos Kanellopoulos" label="Kanellos Kanellopoulos"/>
