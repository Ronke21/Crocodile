<link wikidata="Q24949052" start="85" end="116" title="Singareni Thermal Power Project" label="Singareni Thermal Power Project"/>
<link wikidata="Q2279219" start="190" end="217" title="psychodynamic psychotherapy" label="psychodynamic psychotherapy"/>
<link wikidata="Q7571373" start="267" end="286" title="planning permission" label="planning permission"/>
