<link wikidata="Q5309893" start="41" end="51" title="Drägerman" label="Draegerman"/>
<link wikidata="Q5309893" start="55" end="67" title="Drägerman" label="Draegerwoman"/>
<link wikidata="Q26998165" start="142" end="167" title="Alexander Bernhard Dräger" label="Alexander Bernhard Dräger"/>
<link wikidata="Q490236" start="260" end="279" title="self-contained breathing apparatus" label="breathing apparatus"/>
