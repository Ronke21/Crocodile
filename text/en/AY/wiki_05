<link wikidata="Q671859" start="3730" end="3770" title="Old Possum&amp;#x27;s Book of Practical Cats" label="Old Possum&amp;#x27;s Book of Practical Cats"/>
<link wikidata="Q37767" start="3787" end="3798" title="T. S. Eliot" label="T. S. Eliot"/>
<link wikidata="Q298685" start="3812" end="3821" title="Dr. Seuss" label="Dr. Seuss"/>
