<link wikidata="Q381728" start="4127" end="4140" title="George Stubbs" label="George Stubbs"/>
<link wikidata="Q194402" start="4203" end="4218" title="Joshua Reynolds" label="Joshua Reynolds"/>
<link wikidata="Q5595260" start="4271" end="4283" title="Grand Manner" label="Grand Manner"/>
<link wikidata="Q981971" start="4359" end="4369" title="Mario Praz" label="Mario Praz"/>
</links>
