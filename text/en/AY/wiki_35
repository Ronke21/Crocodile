<link wikidata="Q185110" start="9522" end="9536" title="Igor Kurchatov" label="Igor Kurchatov"/>
<link wikidata="Q757644" start="9971" end="9986" title="Atoms for Peace" label="Atoms for Peace"/>
<link wikidata="Q71" start="10001" end="10007" title="Geneva" label="Geneva"/>
