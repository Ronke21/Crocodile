<link wikidata="Q24706" start="4280" end="4285" title="nengō" label="nengō"/>
<link wikidata="Q1143928" start="4301" end="4307" title="Kanshō" label="Kanshō"/>
<link wikidata="Q1193596" start="4334" end="4340" title="Bunshō" label="Bunshō"/>
