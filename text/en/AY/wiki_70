<link wikidata="Q9199" start="154" end="164" title="Vietnamese language" label="Vietnamese"/>
<link wikidata="Q36172" start="166" end="170" title="Moro language" label="Moro"/>
<link wikidata="Q56463" start="176" end="190" title="Sámi languages" label="Sámi languages"/>
<link wikidata="Q165795" start="199" end="212" title="Kven language" label="Kven language"/>
<link wikidata="Q9993" start="250" end="253" title="Eth" label="Eth"/>
<link wikidata="Q294" start="270" end="279" title="Icelandic language" label="Icelandic"/>
