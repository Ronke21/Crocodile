<link wikidata="Q331198" start="2229" end="2244" title="William Gifford" label="William Gifford"/>
<link wikidata="Q45546" start="2249" end="2267" title="William Wordsworth" label="William Wordsworth"/>
<link wikidata="Q725786" start="2308" end="2322" title="Charles Knight (publisher)" label="Charles Knight"/>
