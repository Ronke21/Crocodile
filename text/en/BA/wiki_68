<link wikidata="Q118406" start="4428" end="4435" title="diploid" label="diploid"/>
<link wikidata="Q321362" start="5165" end="5176" title="pinocytosis" label="pinocytosis"/>
<link wikidata="Q191934" start="5298" end="5307" title="commensal" label="commensal"/>
<link wikidata="Q10811" start="6059" end="6066" title="reptile" label="reptile"/>
<link wikidata="Q53663" start="6070" end="6080" title="salamander" label="salamander"/>
