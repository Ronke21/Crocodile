<link wikidata="Q5133090" start="61" end="75" title="Clifford torus" label="Clifford torus"/>
<link wikidata="Q12510" start="107" end="112" title="torus" label="torus"/>
<link wikidata="Q2274197" start="120" end="128" title="3-sphere" label="3-sphere"/>
