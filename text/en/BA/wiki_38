<link wikidata="Q369377" start="924" end="948" title="total order" label="strictly totally ordered"/>
<link wikidata="Q216601" start="1152" end="1158" title="Object (computer science)" label="object"/>
<link wikidata="Q3990742" start="2067" end="2084" title="Thomas Write Rule" label="Thomas Write Rule"/>
<link wikidata="Q3990742" start="4434" end="4451" title="Thomas write rule" label="Thomas write rule"/>
<link wikidata="Q93695" start="6931" end="6963" title="Multiversion concurrency control" label="Multiversion concurrency control"/>
<link wikidata="Q7806609" start="6966" end="6990" title="Timestamping (computing)" label="Timestamping (computing)"/>
</links>
