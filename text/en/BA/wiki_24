<link wikidata="Q5185622" start="6038" end="6052" title="criminal jurisdiction" label="jurisdictional"/>
<link wikidata="Q246624" start="6172" end="6187" title="Sixth Amendment to the U.S. Constitution" label="Sixth Amendment"/>
<link wikidata="Q651929" start="6356" end="6391" title="Federal Rules of Criminal Procedure" label="Federal Rules of Criminal Procedure"/>
