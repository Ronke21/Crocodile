<link wikidata="Q547414" start="4791" end="4803" title="Algis Budrys" label="Algis Budrys"/>
<link wikidata="Q5099499" start="5122" end="5137" title="SF Encyclopedia" label="SF Encyclopedia"/>
<link wikidata="Q208800" start="5257" end="5267" title="Lamarckism" label="Lamarckian"/>
