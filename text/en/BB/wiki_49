<link wikidata="Q198438" start="95" end="101" title="zigzag" label="zigzag"/>
<link wikidata="Q1314278" start="841" end="846" title="woven" label="woven"/>
<link wikidata="Q28823" start="847" end="852" title="cloth" label="cloth"/>
