<link wikidata="Q668113" start="29" end="40" title="Hexahedron" label="hexahedrons"/>
<link wikidata="Q668113" start="45" end="55" title="Hexahedron" label="sexahedron"/>
<link wikidata="Q668113" start="59" end="68" title="Hexahedron" label="sexahedra"/>
