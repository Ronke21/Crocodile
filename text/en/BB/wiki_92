<link wikidata="Q4947789" start="321" end="345" title="Boston Bulldogs (soccer)" label="Boston Bulldogs (soccer)"/>
<link wikidata="Q4816473" start="492" end="520" title="Boston Bulldogs (ice hockey)" label="Boston Bulldogs (ice hockey)"/>
<link wikidata="None" start="626" end="649" title="Boston Bulldogs (WBCBL)" label="Boston Bulldogs (WBCBL)"/>
