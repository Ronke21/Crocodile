<link wikidata="Q35505" start="32" end="41" title="Old Norse" label="Old Norse"/>
<link wikidata="Q178182" start="81" end="87" title="Asgard" label="Asgard"/>
<link wikidata="Q43610" start="94" end="98" title="Odin" label="Odin"/>
<link wikidata="Q177860" start="114" end="122" title="Valhalla" label="Valhalla"/>
<link wikidata="Q1429923" start="153" end="163" title="Grímnismál" label="Grímnismál"/>
<link wikidata="Q102323" start="172" end="178" title="Snorri Sturluson" label="Snorri"/>
<link wikidata="Q1134288" start="195" end="207" title="Gylfaginning" label="Gylfaginning"/>
