<link wikidata="Q511056" start="73" end="82" title="Solar deity" label="solar god"/>
<link wikidata="Q215304" start="219" end="232" title="creation myth" label="creation myth"/>
<link wikidata="Q220264" start="242" end="251" title="Popol Vuh" label="Popol Vuh"/>
</links>
