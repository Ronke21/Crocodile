<link wikidata="Q861799" start="6716" end="6723" title="Orion Publishing Group" label="Cassell"/>
<link wikidata="Q1203584" start="6797" end="6815" title="Deutsche Mythologie" label="Teutonic Mythology"/>
<link wikidata="Q4382004" start="6870" end="6886" title="Benjamin Thorpe" label="Thorpe, Benjamin"/>
