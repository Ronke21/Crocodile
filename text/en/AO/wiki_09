<link wikidata="Q184661" start="3536" end="3541" title="Ogham" label="Ogham"/>
<link wikidata="Q1106575" start="3697" end="3713" title="Celtic mythology" label="Celtic mythology"/>
<link wikidata="Q465434" start="3776" end="3786" title="Celtic god" label="Celtic god"/>
<link wikidata="Q122248" start="3935" end="3943" title="Herakles" label="Herakles"/>
<link wikidata="Q177847" start="4031" end="4037" title="Lucian" label="Lucian"/>
<link wikidata="Q13247" start="4358" end="4375" title="Rudolf Thurneysen" label="Rudolf Thurneysen"/>
<link wikidata="Q4647811" start="4380" end="4395" title="A. G. van Hamel" label="Anton van Hamel"/>
<link wikidata="Q5406861" start="4438" end="4452" title="Eufydd fab Dôn" label="Eufydd fab Dôn"/>
<link wikidata="Q5406861" start="4598" end="4612" title="Eufydd fab Dôn" label="Eufydd fab Dôn"/>
<link wikidata="Q1399542" start="4614" end="4620" title="Ogmios" label="Ogmios"/>
