<link wikidata="Q1075793" start="143" end="164" title="Saint-Andéol, Drôme" label="Saint-Andéol-en-Quint"/>
<link wikidata="Q632455" start="169" end="185" title="Rochefort-Samson" label="Rochefort-Samson"/>
