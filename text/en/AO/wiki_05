<link wikidata="Q2404442" start="123" end="132" title="Indigetes" label="Indigetes"/>
<link wikidata="Q2422375" start="181" end="207" title="Roman conquest of Hispania" label="Roman conquest of Hispania"/>
<link wikidata="Q443983" start="250" end="273" title="Aulus Postumius Albinus (consul 151 BC)" label="Aulus Postumius Albinus"/>
<link wikidata="Q216085" start="715" end="727" title="Viminal Hill" label="Viminal Hill"/>
