<link wikidata="Q908438" start="54" end="61" title="hyaline" label="hyaline"/>
<link wikidata="Q15467261" start="62" end="74" title="Degeneration (medical)" label="degeneration"/>
<link wikidata="Q178069" start="78" end="86" title="necrosis" label="necrosis"/>
<link wikidata="Q7881" start="90" end="98" title="skeleton" label="skeletal"/>
<link wikidata="Q65505" start="202" end="229" title="Friedrich Albert von Zenker" label="Friedrich Albert von Zenker"/>
<link wikidata="Q275150" start="288" end="304" title="rectus abdominis" label="rectus abdominis"/>
