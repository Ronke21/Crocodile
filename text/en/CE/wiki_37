<link wikidata="Q962165" start="159" end="174" title="formal fallacy" label="logical fallacy"/>
<link wikidata="Q188908" start="578" end="595" title="Murphy&amp;#x27;s law" label="Murphy&amp;#x27;s law"/>
<link wikidata="Q3993049" start="624" end="639" title="tongue-in-cheek" label="tongue-in-cheek"/>
<link wikidata="Q876455" start="684" end="698" title="Slippery slope" label="Slippery slope"/>
