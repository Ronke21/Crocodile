<link wikidata="Q37226" start="93" end="100" title="teacher" label="teacher"/>
<link wikidata="Q160879" start="136" end="146" title="Boizenburg" label="Boizenburg"/>
<link wikidata="Q1644" start="163" end="167" title="Elbe" label="Elbe"/>
