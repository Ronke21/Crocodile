<link wikidata="Q7758202" start="580" end="598" title="The Priests of Psi" label="The Priests of Psi"/>
<link wikidata="Q742918" start="1571" end="1586" title="Colin Greenland" label="Colin Greenland"/>
<link wikidata="Q6002663" start="1632" end="1639" title="Imagine (game magazine)" label="Imagine"/>
