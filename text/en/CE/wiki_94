<link wikidata="Q15777" start="3599" end="3600" title="C (programming language)" label="C"/>
<link wikidata="Q1050390" start="3608" end="3614" title="malloc" label="malloc"/>
<link wikidata="Q657179" start="3725" end="3739" title="memory-mapped file" label="explicitly map"/>
