<link wikidata="Q185467" start="36" end="49" title="socialization" label="socialization"/>
<link wikidata="Q7566" start="100" end="107" title="parents" label="parents"/>
<link wikidata="Q1058791" start="388" end="397" title="Synchronization" label="synchrony"/>
<link wikidata="Q998" start="620" end="627" title="infancy" label="infancy"/>
