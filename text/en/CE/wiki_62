<link wikidata="Q947090" start="419" end="423" title="Best Pop Vocal Performance, Male" label="male"/>
<link wikidata="Q935283" start="428" end="434" title="Best Pop Vocal Performance, Female" label="female"/>
<link wikidata="Q747902" start="452" end="466" title="Best Pop Vocal Performance by a Duo or Group" label="duos or groups"/>
