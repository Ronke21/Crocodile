<link wikidata="Q5593779" start="231" end="270" title="Grammy Award for Best Contemporary Song" label="Best Performance by a Pop Single Artist"/>
<link wikidata="Q5593779" start="309" end="339" title="Grammy Award for Best Contemporary Song" label="Best Rock &amp;amp; Roll Recording"/>
<link wikidata="Q5593779" start="368" end="397" title="Grammy Award for Best Contemporary Song" label="Contemporary (R&amp;amp;R) Single"/>
<link wikidata="Q5593779" start="426" end="463" title="Grammy Award for Best Contemporary Song" label="Best Contemporary (R&amp;amp;R) Recording"/>
