<link wikidata="Q7447747" start="1412" end="1426" title="Selective Training and Service Act of 1940" label="the draft bill"/>
<link wikidata="Q43297" start="1517" end="1521" title="Time (magazine)" label="Time"/>
<link wikidata="Q361373" start="1809" end="1823" title="Earle Clements" label="Earle Clements"/>
</links>
