<link wikidata="" start="10722" end="10768" title="Mortal Kombat Legends: Scorpion&amp;#x27;s Revenge" label="Mortal Kombat Legends: Scorpion&amp;#x27;s Revenge"/>
<link wikidata="Q117830073" start="10907" end="10922" title="Mortal Kombat 2 (film)" label="Mortal Kombat 2"/>
<link wikidata="Q2710110" start="10963" end="10972" title="GameDaily" label="GameDaily"/>
<link wikidata="Q27197" start="11058" end="11071" title="Game Informer" label="Game Informer"/>
