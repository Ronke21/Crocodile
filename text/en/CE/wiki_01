<link wikidata="Q1393937" start="4024" end="4048" title="Leucocoprinus birnbaumii" label="Leucocoprinus birnbaumii"/>
<link wikidata="Q458004" start="4200" end="4218" title="James Edward Smith (botanist)" label="James Edward Smith"/>
