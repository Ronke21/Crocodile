<link wikidata="Q606042" start="19436" end="19455" title="Glamorgan Wanderers RFC" label="Glamorgan Wanderers"/>
<link wikidata="Q1152808" start="19558" end="19570" title="Stradey Park" label="Stradey Park"/>
