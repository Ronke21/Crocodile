<link wikidata="Q21199" start="85" end="100" title="natural numbers" label="natural numbers"/>
<link wikidata="Q163310" start="215" end="229" title="Turing machine" label="Turing machine"/>
<link wikidata="Q269878" start="1086" end="1087" title="big-O notation" label="O"/>
<link wikidata="Q269878" start="2315" end="2316" title="big-O notation" label="O"/>
<link wikidata="Q269878" start="2898" end="2899" title="little-o notation" label="o"/>
<link wikidata="Q5251122" start="3186" end="3208" title="time hierarchy theorem" label="time hierarchy theorem"/>
