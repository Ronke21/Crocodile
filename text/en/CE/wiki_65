<link wikidata="Q1343885" start="6116" end="6131" title="ITESM Campus Santa Fe" label="Campus Santa Fe"/>
<link wikidata="Q851087" start="6180" end="6183" title="National Action Party (Mexico)" label="PAN"/>
<link wikidata="Q5162816" start="6717" end="6724" title="CONACYT" label="CONACYT"/>
<link wikidata="Q112872019" start="6778" end="6802" title="Exportadora de Sal S.A." label="Exportadora de Sal, S.A."/>
<link wikidata="Q1814176" start="7280" end="7288" title="Sigma Xi" label="Sigma Xi"/>
<link wikidata="Q7181538" start="7293" end="7306" title="Phi Kappa Phi" label="Phi Kappa Phi"/>
