<link wikidata="Q5188562" start="60" end="84" title="Crossed extensor reflex" label="crossed extension reflex"/>
<link wikidata="Q179405" start="90" end="96" title="reflex" label="reflex"/>
<link wikidata="Q7067309" start="202" end="218" title="Noxious stimulus" label="painful stimulus"/>
<link wikidata="Q3454731" start="224" end="241" title="withdrawal reflex" label="withdrawal reflex"/>
<link wikidata="Q5768011" start="275" end="281" title="flexor" label="flexor"/>
<link wikidata="Q5768005" start="324" end="332" title="extensor" label="extensor"/>
