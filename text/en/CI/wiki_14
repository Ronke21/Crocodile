<link wikidata="Q12861" start="58" end="67" title="gunpowder" label="gunpowder"/>
<link wikidata="Q474100" start="169" end="180" title="pyrotechnic" label="pyrotechnic"/>
<link wikidata="Q127933" start="258" end="267" title="fireworks" label="fireworks"/>
<link wikidata="Q12861" start="393" end="410" title="Serpentine powder" label="Serpentine powder"/>
<link wikidata="Q177836" start="489" end="506" title="potassium nitrate" label="potassium nitrate"/>
<link wikidata="Q177463" start="508" end="516" title="charcoal" label="charcoal"/>
