<link wikidata="Q19409" start="21" end="41" title="Hong Kong Film Award" label="Hong Kong Film Award"/>
<link wikidata="Q756683" start="88" end="110" title="Hong Kong Film Archive" label="Hong Kong Film Archive"/>
