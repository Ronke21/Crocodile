<link wikidata="Q6804572" start="160" end="185" title="Mechanics&amp;#x27; Institute" label="Mechanics&amp;#x27; Institute"/>
<link wikidata="Q7935762" start="209" end="224" title="Lord Gormanston" label="Lord Gormanston"/>
<link wikidata="Q8013957" start="235" end="248" title="William Keogh" label="William Keogh"/>
