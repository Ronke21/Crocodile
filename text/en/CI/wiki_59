<link wikidata="Q234943" start="153" end="178" title="total internal reflection" label="total internal reflection"/>
<link wikidata="Q174102" start="356" end="377" title="indices of refraction" label="indices of refraction"/>
<link wikidata="Q208391" start="383" end="399" title="Snell&amp;#x27;s law" label="Snell&amp;#x27;s law"/>
