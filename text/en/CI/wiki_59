<link wikidata="Q2079" start="156" end="163" title="Leipzig" label="Leipzig"/>
<link wikidata="Q20127" start="397" end="415" title="Wilhelm Liebknecht" label="Wilhelm Liebknecht"/>
<link wikidata="Q9061" start="432" end="441" title="Karl Marx" label="Karl Marx"/>
<link wikidata="Q34787" start="446" end="462" title="Friedrich Engels" label="Friedrich Engels"/>
<link wikidata="Q60972" start="542" end="557" title="Joseph Dietzgen" label="Joseph Dietzgen"/>
