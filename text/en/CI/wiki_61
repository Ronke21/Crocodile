<link wikidata="Q7246122" start="294" end="302" title="Dumnonii" label="Dumnonii"/>
<link wikidata="Q5519116" start="662" end="670" title="P-Celtic" label="P-Celtic"/>
<link wikidata="None" start="712" end="719" title="Galeóin" label="Galeóin"/>
<link wikidata="Q56433" start="826" end="834" title="Goidelic" label="Goidelic"/>
<link wikidata="Q159736" start="1069" end="1077" title="Leinster" label="Leinster"/>
