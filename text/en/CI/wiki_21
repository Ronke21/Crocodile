<link wikidata="Q900977" start="60" end="70" title="Payphone" label="pay phones"/>
<link wikidata="None" start="648" end="652" title="Traffic Service Position System#Automated Coin Toll System" label="ACTS"/>
<link wikidata="Q1417360" start="840" end="854" title="induction coil" label="induction coil"/>
<link wikidata="Q4090608" start="1448" end="1463" title="Phreaking boxes" label="Phreaking boxes"/>
