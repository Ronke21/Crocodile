<link wikidata="Q23767" start="3014" end="3031" title="calcium carbonate" label="calcium carbonate"/>
<link wikidata="Q409251" start="3037" end="3052" title="calcium acetate" label="calcium acetate"/>
<link wikidata="Q407125" start="3065" end="3083" title="Aluminium hydroxide" label="aluminum hydroxide"/>
