<link wikidata="Q3304533" start="4007" end="4021" title="Eleventh grade" label="Eleventh grade"/>
<link wikidata="Q986388" start="4024" end="4037" title="Twelfth grade" label="Twelfth grade"/>
<link wikidata="Q8050872" start="4040" end="4048" title="Year Ten" label="Year Ten"/>
<link wikidata="Q19599879" start="4051" end="4077" title="Senior secondary education" label="Senior secondary education"/>
