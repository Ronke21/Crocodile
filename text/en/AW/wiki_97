<link wikidata="Q2914617" start="28872" end="28887" title="Bhakti movement" label="Bhakti movement"/>
<link wikidata="Q173273" start="28954" end="28967" title="Chinmayananda" label="Chinmayananda"/>
<link wikidata="Q7653253" start="29053" end="29065" title="Tapasyananda" label="Tapasyananda"/>
