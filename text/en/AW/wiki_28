<link wikidata="Q30" start="7449" end="7462" title="United States" label="United States"/>
<link wikidata="Q252" start="7485" end="7494" title="Indonesia" label="Indonesia"/>
<link wikidata="Q44819" start="7517" end="7524" title="Suharto" label="Suharto"/>
