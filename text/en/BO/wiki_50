<link wikidata="Q901718" start="303" end="310" title="product (mathematics)" label="product"/>
<link wikidata="Q1925224" start="371" end="384" title="failure causes" label="failure modes"/>
<link wikidata="Q625303" start="401" end="426" title="statistically independent" label="statistically independent"/>
<link wikidata="None" start="731" end="747" title="Erich Pieruschka" label="Erich Pieruschka"/>
</links>
