<link wikidata="Q6747553" start="23190" end="23217" title="Manchester University Press" label="Manchester University Press"/>
<link wikidata="Q3044933" start="23288" end="23300" title="G. D. H. Cole" label="Cole, G.D.H."/>
<link wikidata="Q7344720" start="23663" end="23676" title="Robert George Gammage" label="Gammage, R.G."/>
<link wikidata="Q719365" start="23734" end="23748" title="George Holyoake" label="Holyoake, G.J."/>
<link wikidata="Q7610383" start="24119" end="24135" title="Stephen Roberts (historian)" label="Roberts, Stephen"/>
