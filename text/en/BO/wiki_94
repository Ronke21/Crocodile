<link wikidata="Q5312901" start="23125" end="23154" title="Duke University School of Law" label="Duke University School of Law"/>
<link wikidata="Q845768" start="23159" end="23177" title="Cornell Law School" label="Cornell Law School"/>
<link wikidata="Q7895892" start="23291" end="23336" title="University of Nebraska-Lincoln College of Law" label="University of Nebraska-Lincoln College of Law"/>
