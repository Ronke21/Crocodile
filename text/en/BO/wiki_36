<link wikidata="Q204260" start="50" end="60" title="hand plane" label="hand plane"/>
<link wikidata="Q1423948" start="175" end="184" title="Wood grain" label="end grain"/>
<link wikidata="Q938509" start="992" end="999" title="cleaver (knife)" label="cleaver"/>
<link wikidata="Q1115418" start="1011" end="1032" title="Butcher block" label="butchers&amp;#x27; blocks"/>
<link wikidata="Q1397389" start="2013" end="2020" title="Chamfer" label="Chamfer"/>
<link wikidata="Q131790" start="2060" end="2064" title="Adhesive" label="glue"/>
