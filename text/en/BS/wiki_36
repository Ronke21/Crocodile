<link wikidata="Q290356" start="41" end="63" title="San Pedro, Los Angeles" label="San Pedro, Los Angeles"/>
<link wikidata="None" start="89" end="102" title="Barbecue in the United States#California" label="barbecue pits"/>
<link wikidata="Q961082" start="106" end="112" title="gazebo" label="gazebo"/>
