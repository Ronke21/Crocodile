<link wikidata="Q3176518" start="3352" end="3364" title="Jeff Kennett" label="Jeff Kennett"/>
<link wikidata="Q959320" start="4322" end="4339" title="doctorate of laws" label="doctorate of laws"/>
<link wikidata="Q598841" start="4345" end="4362" title="Monash University" label="Monash University"/>
