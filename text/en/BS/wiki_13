<link wikidata="Q695161" start="2481" end="2491" title="Amiga CD32" label="Amiga CD32"/>
<link wikidata="Q4035600" start="2504" end="2508" title="CDXL" label="CDXL"/>
<link wikidata="Q869598" start="3463" end="3479" title="double buffering" label="double buffering"/>
