<link wikidata="Q25439" start="4686" end="4696" title="woodpecker" label="woodpecker"/>
<link wikidata="Q856531" start="5007" end="5027" title="Greater racket-tailed drongo" label="racket-tailed drongo"/>
<link wikidata="Q130948" start="5220" end="5230" title="Plasmodium" label="Plasmodium"/>
<link wikidata="Q5638270" start="5263" end="5275" title="Haemoproteus" label="Haemoproteus"/>
<link wikidata="Q4267416" start="5939" end="5949" title="Lotha Naga" label="Lotha Naga"/>
