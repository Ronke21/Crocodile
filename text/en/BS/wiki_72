<link wikidata="Q927864" start="45" end="54" title="Ferryland" label="Ferryland"/>
<link wikidata="Q25" start="89" end="94" title="Wales" label="Wales"/>
<link wikidata="Q335412" start="116" end="134" title="George Calvert, 1st Baron Baltimore" label="Sir George Calvert"/>
