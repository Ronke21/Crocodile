<link wikidata="Q5176992" start="498" end="515" title="Countermeasure (law)" label="International Law"/>
<link wikidata="Q378426" start="518" end="528" title="Diplomatics" label="Diplomatic"/>
<link wikidata="None" start="1362" end="1381" title="Security Technology" label="Security Technology"/>
<link wikidata="Q2094686" start="1734" end="1748" title="Damage Control" label="Damage Control"/>
<link wikidata="Q57821" start="2015" end="2028" title="Fortification" label="Fortification"/>
