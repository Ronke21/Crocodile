<link wikidata="Q6027" start="77" end="96" title="Les Rougon-Macquart" label="Les Rougon-Macquart"/>
<link wikidata="Q962265" start="195" end="203" title="La Curée" label="La Curée"/>
<link wikidata="Q607232" start="227" end="240" title="L&amp;#x27;Argent" label="L&amp;#x27;Argent"/>
