<link wikidata="Q1909207" start="56" end="68" title="Matt Johnson (singer)" label="Matt Johnson"/>
<link wikidata="Q1354463" start="303" end="310" title="The The" label="The The"/>
<link wikidata="Q238095" start="335" end="338" title="4AD" label="4AD"/>
<link wikidata="Q4977553" start="1118" end="1130" title="Bruce Gilbert" label="B.C. Gilbert"/>
<link wikidata="Q5593023" start="1137" end="1145" title="Graham Lewis" label="G. Lewis"/>
