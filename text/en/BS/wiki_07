<link wikidata="Q1331373" start="4927" end="4948" title="strongly inaccessible" label="strongly inaccessible"/>
<link wikidata="Q7243376" start="5040" end="5051" title="prime model" label="prime model"/>
<link wikidata="Q877149" start="5293" end="5313" title="elementary embedding" label="elementary embedding"/>
<link wikidata="Q467606" start="5652" end="5663" title="homogeneous model" label="homogeneous"/>
<link wikidata="Q7595779" start="5946" end="5952" title="stable theory" label="stable"/>
