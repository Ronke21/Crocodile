<link wikidata="Q57900" start="7691" end="7706" title="U.S. Bank Tower" label="U.S. Bank Tower"/>
<link wikidata="Q105387" start="7798" end="7814" title="Independence Day (1996 film)" label="Independence Day"/>
<link wikidata="Q14320" start="7857" end="7879" title="The Day After Tomorrow" label="The Day After Tomorrow"/>
