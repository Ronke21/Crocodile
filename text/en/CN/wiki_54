<link wikidata="Q3698751" start="14167" end="14173" title="Crucis (band)" label="Crucis"/>
<link wikidata="Q5360967" start="14217" end="14225" title="Espíritu (band)" label="Espíritu"/>
<link wikidata="Q6464071" start="14334" end="14361" title="La Máquina de Hacer Pájaros" label="La Máquina de Hacer Pájaros"/>
<link wikidata="Q3698751" start="14517" end="14523" title="Crucis (band)" label="Crucis"/>
<link wikidata="Q2577277" start="14578" end="14583" title="Pappo" label="Pappo"/>
