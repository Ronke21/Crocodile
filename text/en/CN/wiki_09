<link wikidata="Q338810" start="152" end="164" title="lead acetate" label="lead acetate"/>
<link wikidata="Q422837" start="220" end="236" title="lead(II) acetate" label="lead(II) acetate"/>
<link wikidata="Q407879" start="241" end="255" title="lead(II) oxide" label="lead(II) oxide"/>
<link wikidata="Q378705" start="267" end="277" title="astringent" label="astringent"/>
