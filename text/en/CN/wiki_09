<link wikidata="Q36727" start="25" end="29" title="Pali" label="Pali"/>
<link wikidata="Q1939884" start="34" end="42" title="Bodhimaṇḍa" label="daochang"/>
<link wikidata="Q748" start="98" end="106" title="Buddhism" label="Buddhism"/>
