<link wikidata="Q173115" start="1495" end="1510" title="Elio de Angelis" label="Elio de Angelis"/>
<link wikidata="Q78489" start="1529" end="1539" title="Niki Lauda" label="Niki Lauda"/>
<link wikidata="Q172030" start="1547" end="1554" title="McLaren (racing)" label="McLaren"/>
<link wikidata="Q171346" start="1623" end="1636" title="Nelson Piquet" label="Nelson Piquet"/>
<link wikidata="Q63928692" start="1689" end="1692" title="BMW in Formula One" label="BMW"/>
<link wikidata="Q172775" start="1964" end="1978" title="Jody Scheckter" label="Jody Scheckter"/>
<link wikidata="Q65483" start="2005" end="2029" title="1980 Canadian Grand Prix" label="1980 Canadian Grand Prix"/>
<link wikidata="Q173112" start="2352" end="2368" title="Bruno Giacomelli" label="Bruno Giacomelli"/>
<link wikidata="Q57733" start="2389" end="2407" title="Manfred Winkelhock" label="Manfred Winkelhock"/>
<link wikidata="Q784953" start="2535" end="2538" title="ATS (wheels)" label="ATS"/>
<link wikidata="Q126940" start="2755" end="2771" title="Roberto Guerrero" label="Roberto Guerrero"/>
<link wikidata="Q173415" start="2779" end="2785" title="Ensign (racing team)" label="Ensign"/>
<link wikidata="Q57459" start="2995" end="3006" title="Jochen Mass" label="Jochen Mass"/>
<link wikidata="Q172759" start="3067" end="3083" title="Riccardo Patrese" label="Riccardo Patrese"/>
<link wikidata="Q173279" start="4033" end="4044" title="René Arnoux" label="René Arnoux"/>
