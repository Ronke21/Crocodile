<link wikidata="Q85751410" start="5534" end="5554" title="Champ Car" label="CART Indy Car series"/>
<link wikidata="Q171992" start="5557" end="5564" title="Tyrrell Racing" label="Tyrrell"/>
<link wikidata="Q172983" start="5572" end="5588" title="Michele Alboreto" label="Michele Alboreto"/>
