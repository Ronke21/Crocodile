<link wikidata="Q2166873" start="124" end="166" title="Royal National Lifeboat Institution" label="Royal National Lifeboat Institution (RNLI)"/>
<link wikidata="Q1361551" start="224" end="232" title="Lifeboat (rescue)" label="lifeboat"/>
<link wikidata="Q716482" start="280" end="299" title="Detonator (railway)" label="signal on a railway"/>
