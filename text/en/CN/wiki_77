<link wikidata="Q475241" start="263" end="274" title="speedometer" label="speedometer"/>
<link wikidata="Q1417134" start="288" end="297" title="dashboard" label="dashboard"/>
<link wikidata="Q1420" start="304" end="314" title="automobile" label="automobile"/>
