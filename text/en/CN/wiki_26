<link wikidata="Q131691" start="98" end="138" title="Arthur Wellesley, 1st Duke of Wellington" label="Arthur Wellesley, the Duke of Wellington"/>
<link wikidata="Q181875" start="199" end="214" title="Robert Peel" label="Sir Robert Peel"/>
<link wikidata="Q165154" start="228" end="236" title="Kingdom of Sardinia" label="Sardinia"/>
<link wikidata="Q3112663" start="312" end="330" title="Caretaker government" label="caretaker capacity"/>
