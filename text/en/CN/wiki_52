<link wikidata="Q7889" start="19" end="43" title="Video game" label="computer and video games"/>
<link wikidata="Q60" start="95" end="108" title="New York City" label="New York City"/>
<link wikidata="Q15712005" start="114" end="128" title="fictional city" label="fictional city"/>
