<link wikidata="Q190438" start="46" end="51" title="dwarf galaxy" label="dwarf"/>
<link wikidata="Q190397" start="52" end="68" title="irregular galaxy" label="irregular galaxy"/>
<link wikidata="Q3944" start="182" end="193" title="Local Group" label="Local Group"/>
<link wikidata="Q321" start="226" end="235" title="Milky Way" label="Milky Way"/>
<link wikidata="Q59973" start="667" end="691" title="Gran Telescopio Canarias" label="Gran Telescopio Canarias"/>
