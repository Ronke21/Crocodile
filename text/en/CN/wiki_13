<link wikidata="Q3461467" start="15010" end="15018" title="Philmont, New York" label="Philmont"/>
<link wikidata="Q2486448" start="15041" end="15047" title="New York State Route 217" label="NY 217"/>
<link wikidata="Q6017969" start="15149" end="15164" title="scenic overlook" label="scenic overlook"/>
<link wikidata="Q2372542" start="15285" end="15290" title="Ghent, New York" label="Ghent"/>
