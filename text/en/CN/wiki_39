<link wikidata="Q82744" start="54" end="70" title="Image scanner" label="flatbed scanners"/>
<link wikidata="Q189880" start="127" end="148" title="Charge-coupled device" label="Charge-coupled device"/>
<link wikidata="Q42378" start="448" end="451" title="USB" label="USB"/>
