<link wikidata="Q43343" start="53" end="62" title="folk song" label="folk song"/>
<link wikidata="None" start="403" end="413" title="W-position" label="W-position"/>
<link wikidata="Q7439" start="443" end="459" title="Richard Stallman" label="Richard Stallman"/>
<link wikidata="Q266295" start="480" end="498" title="Free Software Song" label="Free Software Song"/>
