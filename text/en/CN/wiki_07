<link wikidata="Q2478311" start="31" end="48" title="1981 Brixton riot" label="1981 Brixton riot"/>
<link wikidata="Q4582222" start="67" end="84" title="1985 Brixton riot" label="1985 Brixton riot"/>
<link wikidata="Q3710167" start="107" end="124" title="1995 Brixton riot" label="1995 Brixton riot"/>
