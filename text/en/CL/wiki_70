<link wikidata="Q361" start="140" end="155" title="First World War" label="First World War"/>
<link wikidata="Q7785" start="259" end="271" title="Commonwealth of Nations" label="Commonwealth"/>
<link wikidata="Q5308173" start="441" end="461" title="Battle of Drocourt-Quéant Line" label="Drocourt-Quéant Line"/>
<link wikidata="Q131329" start="467" end="480" title="Arras" label="Arras, France"/>
<link wikidata="Q4798801" start="501" end="521" title="Arthur George Knight" label="Arthur George Knight"/>
<link wikidata="Q8012096" start="523" end="544" title="William Henry Metcalf" label="William Henry Metcalf"/>
