<link wikidata="Q424863" start="125" end="130" title="Omega (Cyrillic)" label="Omega"/>
<link wikidata="Q182438" start="139" end="141" title="Te (Cyrillic)" label="Te"/>
<link wikidata="Q33251" start="339" end="354" title="Church Slavonic" label="Church Slavonic"/>
