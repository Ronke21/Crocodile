<link wikidata="Q501319" start="91" end="105" title="Charles County, Maryland" label="Charles County"/>
<link wikidata="Q1391" start="107" end="115" title="Maryland" label="Maryland"/>
<link wikidata="Q755056" start="182" end="189" title="Waldorf, Maryland" label="Waldorf"/>
<link wikidata="Q180914" start="269" end="286" title="John Wilkes Booth" label="John Wilkes Booth"/>
<link wikidata="Q1484356" start="290" end="305" title="Samuel Mudd" label="Dr. Mudd&amp;#x27;s"/>
