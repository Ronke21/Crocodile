<link wikidata="Q3127696" start="13499" end="13505" title="Harper (publisher)" label="Harper"/>
<link wikidata="Q57788611" start="13855" end="13875" title="The American Scholar" label="The American Scholar"/>
<link wikidata="Q1542536" start="13885" end="13905" title="The Atlantic Monthly" label="The Atlantic Monthly"/>
<link wikidata="Q15708605" start="13908" end="13925" title="The Hudson Review" label="The Hudson Review"/>
