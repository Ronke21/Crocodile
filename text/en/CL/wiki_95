<link wikidata="Q49143" start="11373" end="11393" title="Quincy, Massachusetts" label="colony of Merrymount"/>
<link wikidata="Q176584" start="11425" end="11438" title="Thomas Morton (colonist)" label="Thomas Morton"/>
<link wikidata="Q82708" start="11879" end="11896" title="Church of England" label="Church of England"/>
