<link wikidata="Q315836" start="6299" end="6313" title="Attila Kotányi" label="Attila Kotányi"/>
<link wikidata="Q543582" start="6318" end="6336" title="Jacqueline de Jong" label="Jacqueline de Jong"/>
<link wikidata="Q108481" start="7148" end="7165" title="Dieter Kunzelmann" label="Dieter Kunzelmann"/>
<link wikidata="Q895865" start="7203" end="7217" title="Lothar Fischer" label="Lothar Fischer"/>
