<link wikidata="Q1063649" start="95" end="99" title="Elis (regional unit)" label="Elis"/>
<link wikidata="Q41" start="101" end="107" title="Greece" label="Greece"/>
<link wikidata="Q37495" start="138" end="148" title="Ionian Sea" label="Ionian Sea"/>
<link wikidata="Q13407147" start="163" end="171" title="Krestena" label="Krestena"/>
