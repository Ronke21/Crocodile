<link wikidata="Q1045326" start="19700" end="19717" title="Sergio Scaglietti" label="Sergio Scaglietti"/>
<link wikidata="Q181282" start="19722" end="19734" title="Enzo Ferrari" label="Enzo Ferrari"/>
