<link wikidata="Q6015661" start="40" end="46" title="Matla&amp;#x27;" label="matla″"/>
<link wikidata="Q13955" start="53" end="59" title="Arabic" label="Arabic"/>
<link wikidata="Q2581045" start="91" end="95" title="Bayt (poetry)" label="bayt"/>
<link wikidata="Q281111" start="126" end="132" title="ghazal" label="ghazal"/>
<link wikidata="Q3912956" start="187" end="198" title="maqta&amp;#x27;" label="maqta&amp;#x27;"/>
