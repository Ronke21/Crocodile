<link wikidata="Q81406" start="46" end="52" title="alkene" label="alkene"/>
<link wikidata="Q181898" start="53" end="67" title="polymerization" label="polymerization"/>
<link wikidata="Q413676" start="101" end="112" title="metallocene" label="metallocene"/>
<link wikidata="Q19588" start="125" end="141" title="transition metal" label="transition metal"/>
