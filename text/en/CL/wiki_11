<link wikidata="Q7775728" start="27" end="65" title="The Woodentops (TV series)" label="&amp;quot;The Woodentops&amp;quot; (TV series)"/>
<link wikidata="Q2037805" start="104" end="118" title="The Woodentops" label="The Woodentops"/>
<link wikidata="Q1527859" start="222" end="236" title="Policing in the United Kingdom" label="police officer"/>
