<link wikidata="Q35497" start="32" end="45" title="Ancient Greek" label="Ancient Greek"/>
<link wikidata="Q1465871" start="90" end="99" title="Phthonus" label="[[Zelus]]"/>
<link wikidata="Q312916" start="190" end="196" title="Nonnus" label="Nonnus"/>
<link wikidata="Q634519" start="210" end="220" title="Dionysiaca" label="Dionysiaca"/>
