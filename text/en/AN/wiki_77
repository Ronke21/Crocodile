<link wikidata="Q329924" start="41" end="56" title="Abraham Darby I" label="Abraham Darby I"/>
<link wikidata="Q329920" start="254" end="270" title="Abraham Darby II" label="Abraham Darby II"/>
<link wikidata="Q329925" start="335" end="352" title="Abraham Darby III" label="Abraham Darby III"/>
<link wikidata="Q4668827" start="509" end="525" title="Abraham Darby IV" label="Abraham Darby IV"/>
