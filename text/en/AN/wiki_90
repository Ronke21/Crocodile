<link wikidata="Q1386232" start="71805" end="71819" title="Owen Chadwick" label="Chadwick, Owen"/>
<link wikidata="Q5592984" start="72771" end="72784" title="Graham Kings" label="Kings, Graham"/>
<link wikidata="Q540427" start="73265" end="73281" title="Lytton Strachey" label="Strachey, Lytton"/>
<link wikidata="Q6819481" start="73353" end="73367" title="Meriol Trevor" label="Trevor, Meriol"/>
