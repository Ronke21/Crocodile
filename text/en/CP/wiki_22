<link wikidata="Q5297" start="9955" end="9970" title="Microprocessor" label="microprocessors"/>
<link wikidata="Q179645" start="10056" end="10064" title="Shinjuku" label="Shinjuku"/>
<link wikidata="Q1490" start="10077" end="10082" title="Tokyo" label="Tokyo"/>
<link wikidata="Q1343212" start="10126" end="10142" title="Enix" label="Enix Corporation"/>
<link wikidata="Q1457152" start="10190" end="10204" title="Sega Sammy Holdings" label="Sammy Industry"/>
<link wikidata="Q5639978" start="10232" end="10245" title="Hajime Satomi" label="Hajime Satomi"/>
