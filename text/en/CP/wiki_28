<link wikidata="Q172107" start="134" end="151" title="Polish–Lithuanian Commonwealth" label="Polish–Lithuanian"/>
<link wikidata="Q311995" start="159" end="168" title="szlachta" label="szlachcic"/>
<link wikidata="Q49683" start="209" end="233" title="Grand Duchy of Lithuania" label="Grand Duchy of Lithuania"/>
<link wikidata="Q172107" start="247" end="277" title="Polish–Lithuanian Commonwealth" label="Polish–Lithuanian Commonwealth"/>
<link wikidata="Q275102" start="279" end="286" title="Voivode" label="Voivode"/>
