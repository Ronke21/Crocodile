<link wikidata="Q193446" start="63" end="79" title="packet switching" label="packet switching"/>
<link wikidata="Q331911" start="109" end="126" title="Packet buffering" label="buffering packets"/>
<link wikidata="Q5318" start="197" end="203" title="router (computing)" label="router"/>
