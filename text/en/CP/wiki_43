<link wikidata="Q7945630" start="32415" end="32429" title="W. H. D. Rouse" label="W. H. D. Rouse"/>
<link wikidata="Q645948" start="32431" end="32453" title="Loeb Classical Library" label="Loeb Classical Library"/>
<link wikidata="Q7945630" start="32668" end="32682" title="W. H. D. Rouse" label="W. H. D. Rouse"/>
