<link wikidata="Q1756677" start="73" end="98" title="Stability theory" label="stable equilibrium points"/>
<link wikidata="Q125977" start="106" end="118" title="vector space" label="vector space"/>
<link wikidata="Q190463" start="379" end="396" title="Lagrangian points" label="Lagrangian points"/>
<link wikidata="Q828814" start="1122" end="1134" title="market share" label="market share"/>
