<link wikidata="Q17092363" start="431" end="448" title="in-band signaling" label="in-band signaling"/>
<link wikidata="Q2501541" start="470" end="491" title="out-of-band signaling" label="out-of-band signaling"/>
<link wikidata="Q243961" start="706" end="710" title="PSTN" label="PSTN"/>
